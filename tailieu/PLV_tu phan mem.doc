{\page}\par \pard \qc \sa120 \sl408 {\fs28 \f1 \b Ph\uc0{\u7847}n II\b0 }\par \pard \qc \sa120 \sl408 {\fs28 \f1 \b T\uc0{\u7920} \uc0{\u272}\uc0{\u193}NH GI\uc0{\u193}\b0 {\rtlch\fcs1 \af31507 \ltrch\fcs0 \insrsid10420566\charrsid10420566 {\*\bkmkstart phanIItudanhgiacuanhatruong}}}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \b I. \uc0{\u272}\uc0{\u7862}T V\uc0{\u7844}N \uc0{\u272}\uc0{\u7872}\b0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Tr\uc0{\u432}\uc0{\u7901}ng M\uc0{\u7847}m non H\uc0{\u432}\uc0{\u7899}ng D\uc0{\u432}\uc0{\u417}ng \uc0{\u273}\uc0{\u432}\uc0{\u7907}c th\uc0{\u224}nh l\uc0{\u7853}p v\uc0{\u224} \uc0{\u273}i v\uc0{\u224}o ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng t\uc0{\u7915} th\uc0{\u225}ng 8/2008. Tr\uc0{\u432}\uc0{\u7901}ng n\uc0{\u7857}m trong khu h\uc0{\u224}nh ch\uc0{\u225}nh thu\uc0{\u7897}c X\uc0{\u227} Xu\uc0{\u226}n Th\uc0{\u7841}nh Huy\uc0{\u7879}n Th\uc0{\u7889}ng Nh\uc0{\u7845}t. \uc0{\u272}\uc0{\u432}\uc0{\u7901}ng x\uc0{\u225} thu\uc0{\u7853}n ti\uc0{\u7879}n cho vi\uc0{\u7879}c \uc0{\u273}\uc0{\u432}a \uc0{\u273}\uc0{\u243}n tr\uc0{\u7867}, ph\uc0{\u7847}n \uc0{\u273}\uc0{\u244}ng c\uc0{\u225}c ch\uc0{\u225}u l\uc0{\u224} con c\uc0{\u225}n b\uc0{\u7897} c\uc0{\u244}ng nh\uc0{\u226}n vi\uc0{\u234}n ch\uc0{\u7913}c \uc0{\u273}ang c\uc0{\u244}ng t\uc0{\u225}c t\uc0{\u7841}i khu h\uc0{\u224}nh ch\uc0{\u225}nh. C\uc0{\u417} s\uc0{\u7903} v\uc0{\u7853}t ch\uc0{\u7845}t khang trang s\uc0{\u7841}ch s\uc0{\u7869}, tho\uc0{\u225}ng m\uc0{\u225}t \uc0{\u273}\uc0{\u225}p \uc0{\u7913}ng nhu c\uc0{\u7847}u Tr\uc0{\u432}\uc0{\u7901}ng M\uc0{\u7847}m Non \uc0{\u273}\uc0{\u7841}t chu\uc0{\u7849}n Qu\uc0{\u7889}c gia. 5 n\uc0{\u259}m li\uc0{\u7873}n \uc0{\u273}\uc0{\u7841}t th\uc0{\u224}nh t\uc0{\u237}ch Tr\uc0{\u432}\uc0{\u7901}ng ti\uc0{\u234}n ti\uc0{\u7871}n c\uc0{\u7845}p ng\uc0{\u224}nh, c\uc0{\u244}ng \uc0{\u273}o\uc0{\u224}n c\uc0{\u417} s\uc0{\u7903} v\uc0{\u7919}ng m\uc0{\u7841}nh, \uc0{\u273}\uc0{\u417}n v\uc0{\u7883} c\uc0{\u243} \uc0{\u273}\uc0{\u7901}i s\uc0{\u7889}ng v\uc0{\u259}n h\uc0{\u243}a xu\uc0{\u7845}t s\uc0{\u7855}c. N\uc0{\u259}m 2014 \uc0{\u273}\uc0{\u432}\uc0{\u7907}c c\uc0{\u244}ng nh\uc0{\u7853}n Tr\uc0{\u432}\uc0{\u7901}ng ti\uc0{\u7871}n ti\uc0{\u7871}n xu\uc0{\u7845}t s\uc0{\u7855}c c\uc0{\u7845}p t\uc0{\u7881}nh, 2 CST\uc0{\u272} c\uc0{\u7845}p t\uc0{\u7881}nh.Tr\uc0{\u432}\uc0{\u7901}ng \uc0{\u273}\uc0{\u7841}t chu\uc0{\u7849}n qu\uc0{\u7889}c gia n\uc0{\u259}m 2013.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \uc0{\u272}\uc0{\u7897}i ng\uc0{\u361} gi\uc0{\u225}o vi\uc0{\u234}n nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng \uc0{\u273}\uc0{\u7847}y \uc0{\u273}\uc0{\u7911}, chu\uc0{\u7849}n v\uc0{\u7873} b\uc0{\u7857}ng c\uc0{\u7845}p theo qui \uc0{\u273}\uc0{\u7883}nh,c\uc0{\u243} tinh th\uc0{\u7847}n tr\uc0{\u225}ch nhi\uc0{\u7879}m v\uc0{\u224} \uc0{\u273}\uc0{\u432}\uc0{\u7907}c \uc0{\u273}\uc0{\u224}o t\uc0{\u7841}o \uc0{\u273}a s\uc0{\u7889} l\uc0{\u224} ch\uc0{\u237}nh qui n\uc0{\u234}n n\uc0{\u7855}m ph\uc0{\u432}\uc0{\u417}ng ph\uc0{\u225}p gi\uc0{\u7843}ng d\uc0{\u7841}y t\uc0{\u432}\uc0{\u417}ng \uc0{\u273}\uc0{\u7889}i t\uc0{\u7889}t, r\uc0{\u232}n luy\uc0{\u7879}n n\uc0{\u226}ng cao tr\uc0{\u236}nh \uc0{\u273}\uc0{\u7897}. S\uc0{\u7889} CB \uc0{\u8211}GV \uc0{\u273}\uc0{\u7841}t tr\uc0{\u236}nh \uc0{\u273}\uc0{\u7897} tr\uc0{\u234}n chu\uc0{\u7849}n \uc0{\u273}\uc0{\u7841}t tr\uc0{\u234}n 80% theo qui \uc0{\u273}\uc0{\u7883}nh. S\uc0{\u7889} GV d\uc0{\u7841}y gi\uc0{\u7887}i C\uc0{\u7845}p Huy\uc0{\u7879}n v\uc0{\u224} t\uc0{\u432}\uc0{\u417}ng \uc0{\u273}\uc0{\u432}\uc0{\u417}ng \uc0{\u273}\uc0{\u7841}t tr\uc0{\u234}n 50% trong n\uc0{\u259}m h\uc0{\u7885}c, tham gia \uc0{\u273}\uc0{\u7847}y \uc0{\u273}\uc0{\u7911} c\uc0{\u225}c h\uc0{\u7897}i thi,c\uc0{\u225}c phong tr\uc0{\u224}o do v\uc0{\u259}n ngh\uc0{\u7879} TDTT do ng\uc0{\u224}nh v\uc0{\u224} c\uc0{\u7845}p tr\uc0{\u234}n ph\uc0{\u225}t \uc0{\u273}\uc0{\u7897}ng.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1  \uc0{\u272}\uc0{\u7897}i ng\uc0{\u361} c\uc0{\u7845}p d\uc0{\u432}\uc0{\u7905}ng tr\uc0{\u7867} nhi\uc0{\u7879}t t\uc0{\u236}nh trong c\uc0{\u244}ng t\uc0{\u225}c, b\uc0{\u7871}p tr\uc0{\u432}\uc0{\u7903}ng c\uc0{\u243} b\uc0{\u7857}ng Trung c\uc0{\u7845}p ch\uc0{\u7871} bi\uc0{\u7871}n \uc0{\u8211} CNTP n\uc0{\u234}n l\uc0{\u432}\uc0{\u7907}ng ch\uc0{\u259}m s\uc0{\u243}c gi\uc0{\u225}o d\uc0{\u7909}c tr\uc0{\u7867} v\uc0{\u224} c\uc0{\u7843}i thi\uc0{\u7879}n c\uc0{\u225}c m\uc0{\u243}n \uc0{\u259}n t\uc0{\u432}\uc0{\u417}ng \uc0{\u273}\uc0{\u7889}i t\uc0{\u7889}t. Ban \uc0{\u272}\uc0{\u7841}i di\uc0{\u7879}n CMHS ph\uc0{\u7889}i h\uc0{\u7907}p c\uc0{\u249}ng nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng t\uc0{\u7893} ch\uc0{\u7913}c t\uc0{\u7889}t c\uc0{\u225}c l\uc0{\u7877} h\uc0{\u7897}i trong nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng. C\uc0{\u243} k\uc0{\u7871} ho\uc0{\u7841}ch thu chi, c\uc0{\u244}ng khai r\uc0{\u245} r\uc0{\u224}ng trong bu\uc0{\u7893}i h\uc0{\u7885}p PHHS t\uc0{\u7841}o s\uc0{\u7921} tin t\uc0{\u432}\uc0{\u7903}ng \uc0{\u7903} ph\uc0{\u7909} huynh trong tr\uc0{\u432}\uc0{\u7901}ng an t\uc0{\u226}m khi g\uc0{\u7903}i tr\uc0{\u7867}. V\uc0{\u236} v\uc0{\u7853}y trong nh\uc0{\u7919}ng n\uc0{\u259}m qua tr\uc0{\u432}\uc0{\u7901}ng \uc0{\u273}\uc0{\u227} \uc0{\u273}\uc0{\u7841}t nh\uc0{\u7919}ng th\uc0{\u224}nh th\uc0{\u224}nh t\uc0{\u237}ch \uc0{\u273}\uc0{\u225}ng ghi nh\uc0{\u7853}n}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \uc0{\u272}\uc0{\u432}\uc0{\u7907}c s\uc0{\u7921} quan t\uc0{\u226}m c\uc0{\u7911}a \uc0{\u272}\uc0{\u7843}ng, Nh\uc0{\u224} n\uc0{\u432}\uc0{\u7899}c, ng\uc0{\u224}nh h\uc0{\u7885}c, gia \uc0{\u273}\uc0{\u236}nh, nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng \uc0{\u273}\uc0{\u7873}u h\uc0{\u432}\uc0{\u7899}ng t\uc0{\u7899}i m\uc0{\u7909}c ti\uc0{\u234}u ph\uc0{\u225}t tri\uc0{\u7875}n n\uc0{\u7873}n gi\uc0{\u225}o d\uc0{\u7909}c, nhu c\uc0{\u7847}u nh\uc0{\u7853}n th\uc0{\u7913}c c\uc0{\u7911}a tr\uc0{\u7867} ng\uc0{\u224}y c\uc0{\u224}ng cao. Sau khi t\uc0{\u7853}p hu\uc0{\u7845}n c\uc0{\u244}ng t\uc0{\u225}c ki\uc0{\u7875}m \uc0{\u273}\uc0{\u7883}nh ch\uc0{\u7845}t l\uc0{\u432}\uc0{\u7907}ng gi\uc0{\u225}o d\uc0{\u7909}c t\uc0{\u7841}i S\uc0{\u7903} Gi\uc0{\u225}o d\uc0{\u7909}c v\uc0{\u224} \uc0{\u272}\uc0{\u224}o t\uc0{\u7841}o \uc0{\u272}\uc0{\u7891}ng Nai, theo Th\uc0{\u244}ng t\uc0{\u432} s\uc0{\u7889} 07/2011/TT-BDG\uc0{\u272}T ng\uc0{\u224}y 17/02/2011 c\uc0{\u7911}a B\uc0{\u7897} Gi\uc0{\u225}o d\uc0{\u7909}c v\uc0{\u224} \uc0{\u272}\uc0{\u224}o t\uc0{\u7841}o. \uc0{\u272}\uc0{\u7875} n\uc0{\u226}ng cao nh\uc0{\u7853}n th\uc0{\u7913}c cho CB, GV, CNV v\uc0{\u7873} c\uc0{\u244}ng t\uc0{\u225}c \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} v\uc0{\u224} ki\uc0{\u7875}m \uc0{\u273}\uc0{\u7883}nh ch\uc0{\u7845}t l\uc0{\u432}\uc0{\u7907}ng gi\uc0{\u225}o d\uc0{\u7909}c, \uc0{\u273}\uc0{\u7891}ng th\uc0{\u7901}i ti\uc0{\u7871}n h\uc0{\u224}nh t\uc0{\u7921} \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} t\uc0{\u7893}ng th\uc0{\u7875} c\uc0{\u225}c ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng gi\uc0{\u225}o d\uc0{\u7909}c c\uc0{\u7911}a nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng trong nh\uc0{\u7919}ng n\uc0{\u259}m qua v\uc0{\u224} c\uc0{\u244}ng t\uc0{\u225}c th\uc0{\u7921}c hi\uc0{\u7879}n s\uc0{\u7855}p t\uc0{\u7899}i, v\uc0{\u7873} \uc0{\u273}\uc0{\u7893}i m\uc0{\u7899}i ph\uc0{\u432}\uc0{\u417}ng ph\uc0{\u225}p gi\uc0{\u7843}ng d\uc0{\u7841}y, qu\uc0{\u7843}n l\uc0{\u253} hi\uc0{\u7879}u qu\uc0{\u7843}, th\uc0{\u7921}c hi\uc0{\u7879}n m\uc0{\u7909}c ti\uc0{\u234}u gi\uc0{\u225}o d\uc0{\u7909}c c\uc0{\u7911}a ng\uc0{\u224}nh m\uc0{\u7847}m non \uc0{\u273}\uc0{\u7863}t ra. }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1  Qua ki\uc0{\u7875}m tra \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} ngo\uc0{\u224}i c\uc0{\u7911}a s\uc0{\u7903} GD&\uc0{\u272}T Tr\uc0{\u432}\uc0{\u7901}ng M\uc0{\u7847}m non H\uc0{\u432}\uc0{\u7899}ng D\uc0{\u432}\uc0{\u417}ng nh\uc0{\u7853}n th\uc0{\u7845}y \uc0{\u273}\uc0{\u432}\uc0{\u7907}c vai tr\uc0{\u242} v\uc0{\u224} tr\uc0{\u225}ch nhi\uc0{\u7879}m c\uc0{\u7911}a t\uc0{\u7915}ng th\uc0{\u224}nh vi\uc0{\u234}n trong nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng. T\uc0{\u7915} \uc0{\u273}\uc0{\u243} coi vi\uc0{\u7879}c t\uc0{\u7921} ki\uc0{\u7875}m tra \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} l\uc0{\u224} vi\uc0{\u7879}c l\uc0{\u224}m th\uc0{\u432}\uc0{\u7901}ng xuy\uc0{\u234}n, c\uc0{\u249}ng nhau ph\uc0{\u7845}n \uc0{\u273}\uc0{\u7845}u \uc0{\u273}\uc0{\u7841}t \uc0{\u273}\uc0{\u432}\uc0{\u7907}c ti\uc0{\u234}u chu\uc0{\u7849}n ch\uc0{\u7845}t l\uc0{\u432}\uc0{\u7907}ng gi\uc0{\u225}o d\uc0{\u7909}c cao h\uc0{\u417}n n\uc0{\u7919}a. }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1    Tr\uc0{\u432}\uc0{\u7901}ng \uc0{\u273}\uc0{\u432}\uc0{\u7907}c UBND T\uc0{\u7881}nh \uc0{\u272}\uc0{\u7891}ng Nai c\uc0{\u244}ng nh\uc0{\u7853}n \uc0{\u273}\uc0{\u7841}t ti\uc0{\u234}u chu\uc0{\u7849}n ch\uc0{\u7845}t l\uc0{\u432}\uc0{\u7907}ng gi\uc0{\u225}o d\uc0{\u7909}c. M\uc0{\u7913}c \uc0{\u273}\uc0{\u7897} II theo Q\uc0{\u272} s\uc0{\u7889} 1621/Q\uc0{\u272}- UBND T\uc0{\u7881}nh \uc0{\u272}\uc0{\u7891}ng Nai ng\uc0{\u224}y 3 th\uc0{\u225}ng 06 n\uc0{\u259}m 2014.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \b II. T\uc0{\u7920} \uc0{\u272}\uc0{\u193}NH GI\uc0{\u193}\b0 {\rtlch\fcs1 \af31507 \ltrch\fcs0 \insrsid10420566\charrsid10420566 {\*\bkmkstart IITudanhgia}}}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \b 1. Ti\uc0{\u234}u chu\uc0{\u7849}n 1: T\uc0{\u244}\uc0{\u777} ch\uc0{\u432}\uc0{\u769}c v\uc0{\u224} qu\uc0{\u7843}n l\uc0{\u253} nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng\b0 {\rtlch\fcs1 \af31507 \ltrch\fcs0 \insrsid10420566\charrsid10420566 {\*\bkmkstart tinhtieuchuan0}}}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \b M\uc0{\u7903} \uc0{\u273}\uc0{\u7847}u:\b0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \uc0{\u272}\uc0{\u432}\uc0{\u7907}c s\uc0{\u7921} quan t\uc0{\u226}m c\uc0{\u7911}a c\uc0{\u225}c c\uc0{\u7845}p l\uc0{\u227}nh \uc0{\u273}\uc0{\u7841}o, Ph\uc0{\u242}ng gi\uc0{\u225}o d\uc0{\u7909}c \uc0{\u273}\uc0{\u224}o t\uc0{\u7841}o, Ch\uc0{\u237}nh quy\uc0{\u7873}n \uc0{\u273}\uc0{\u7883}a ph\uc0{\u432}\uc0{\u417}ng v\uc0{\u224} s\uc0{\u7921} l\uc0{\u7895} l\uc0{\u7921}c c\uc0{\u7911}a t\uc0{\u7853}p th\uc0{\u7875} CB \uc0{\u8211} GV CNV tr\uc0{\u432}\uc0{\u7901}ng MN H\uc0{\u432}\uc0{\u7899}ng D\uc0{\u432}\uc0{\u417}ng m\uc0{\u224} sau khi th\uc0{\u224}nh l\uc0{\u7853}p t\uc0{\u7915} n\uc0{\u259}m 2008 \uc0{\u273}\uc0{\u7871}n nay tr\uc0{\u432}\uc0{\u7901}ng \uc0{\u273}\uc0{\u227} ho\uc0{\u224}n ch\uc0{\u7881}nh v\uc0{\u224} c\uc0{\u417} b\uc0{\u7843}n v\uc0{\u7873} m\uc0{\u7863}t c\uc0{\u417} c\uc0{\u7845}u nh\uc0{\u226}n s\uc0{\u7921} c\uc0{\u361}ng nh\uc0{\u432} \uc0{\u273}\uc0{\u7897}i ng\uc0{\u361} GV \uc0{\u8211} CNV( BGH : 03, c\uc0{\u243} v\uc0{\u259}n th\uc0{\u432}, y t\uc0{\u7871}, GV v\uc0{\u224} CNV \uc0{\u273}\uc0{\u7847}y \uc0{\u273}\uc0{\u7911} theo qui \uc0{\u273}\uc0{\u7883}nh TT 71) 100% CB \uc0{\u8211} GV \uc0{\u273}\uc0{\u227} chu\uc0{\u7849}n theo b\uc0{\u7857}ng c\uc0{\u7845}p (trong \uc0{\u273}\uc0{\u243} tr\uc0{\u234}n chu\uc0{\u7849}n chi\uc0{\u7871}m tr\uc0{\u234}n 80%); \uc0{\u272}\uc0{\u432}\uc0{\u7907}c s\uc0{\u7855}p x\uc0{\u7871}p c\uc0{\u244}ng vi\uc0{\u7879}c \uc0{\u273}\uc0{\u250}ng theo b\uc0{\u7857}ng c\uc0{\u7845}p chuy\uc0{\u234}n m\uc0{\u244}n. Nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng lu\uc0{\u244}n ch\uc0{\u7845}p h\uc0{\u224}nh th\uc0{\u7921}c hi\uc0{\u7879}n theo \uc0{\u273}\uc0{\u250}ng \uc0{\u272}i\uc0{\u7873}u l\uc0{\u7879} Tr\uc0{\u432}\uc0{\u7901}ng M\uc0{\u7847}m non, l\uc0{\u7845}y c\uc0{\u244}ng t\uc0{\u225}c ch\uc0{\u259}m s\uc0{\u243}c gi\uc0{\u225}o d\uc0{\u7909}c tr\uc0{\u7867} l\uc0{\u224} nhi\uc0{\u7879}m v\uc0{\u7909} tr\uc0{\u7885}ng t\uc0{\u226}m h\uc0{\u224}ng \uc0{\u273}\uc0{\u7847}u. Trong c\uc0{\u244}ng t\uc0{\u225}c chuy\uc0{\u234}n m\uc0{\u244}n th\uc0{\u7921}c hi\uc0{\u7879}n nghi\uc0{\u234}m t\uc0{\u250}c theo quy \uc0{\u273}\uc0{\u7883}nh c\uc0{\u7911}a Ph\uc0{\u242}ng Gi\uc0{\u225}o d\uc0{\u7909}c v\uc0{\u224} \uc0{\u272}\uc0{\u224}o t\uc0{\u7841}o, S\uc0{\u7903} Gi\uc0{\u225}o d\uc0{\u7909}c v\uc0{\u224} \uc0{\u272}\uc0{\u224}o t\uc0{\u7841}o \uc0{\u272}\uc0{\u7891}ng Nai. S\uc0{\u7921} ph\uc0{\u7889}i h\uc0{\u7907}p c\uc0{\u7911}a c\uc0{\u225}c \uc0{\u273}o\uc0{\u224}n th\uc0{\u7875} trong nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng th\uc0{\u250}c \uc0{\u273}\uc0{\u7849}y \uc0{\u273}\uc0{\u7875} tr\uc0{\u432}\uc0{\u7901}ng ho\uc0{\u224}n th\uc0{\u224}nh t\uc0{\u7889}t nhi\uc0{\u7879}m v\uc0{\u7909} \uc0{\u273}\uc0{\u432}\uc0{\u7907}c giao v\uc0{\u224} tham gia t\uc0{\u7889}t c\uc0{\u225}c ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng phong tr\uc0{\u224}o do c\uc0{\u225}c ban ng\uc0{\u224}nh, \uc0{\u273}o\uc0{\u224}n th\uc0{\u7875} ph\uc0{\u225}t \uc0{\u273}\uc0{\u7897}ng.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \b \i 1.1. Ti\uc0{\u234}u ch\uc0{\u237} 1: C\uc0{\u417} c\uc0{\u7845}u t\uc0{\u7893} ch\uc0{\u7913}c b\uc0{\u7897} m\uc0{\u225}y c\uc0{\u7911}a nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng theo quy \uc0{\u273}\uc0{\u7883}nh t\uc0{\u7841}i \uc0{\u272}i\uc0{\u7873}u l\uc0{\u7879} tr\uc0{\u432}\uc0{\u7901}ng m\uc0{\u7847}m non.\i0 \b0 {\rtlch\fcs1 \af31507 \ltrch\fcs0 \insrsid10420566\charrsid10420566 {\*\bkmkstart tieuchuan0tieuchi0}}}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \i a) C\uc0{\u243} hi\uc0{\u234}\uc0{\u803}u tr\uc0{\u432}\uc0{\u417}\uc0{\u777}ng, pho\uc0{\u769} hi\uc0{\u234}\uc0{\u803}u tr\uc0{\u432}\uc0{\u417}\uc0{\u777}ng va\uc0{\u768} ca\uc0{\u769}c h\uc0{\u244}\uc0{\u803}i \uc0{\u273}\uc0{\u244}\uc0{\u768}ng (h\uc0{\u244}\uc0{\u803}i \uc0{\u273}\uc0{\u244}\uc0{\u768}ng tr\uc0{\u432}\uc0{\u417}\uc0{\u768}ng \uc0{\u273}\uc0{\u244}\uc0{\u769}i v\uc0{\u417}\uc0{\u769}i tr\uc0{\u432}\uc0{\u417}\uc0{\u768}ng c\uc0{\u244}ng l\uc0{\u226}\uc0{\u803}p, h\uc0{\u244}\uc0{\u803}i \uc0{\u273}\uc0{\u244}\uc0{\u768}ng qua\uc0{\u777}n tri\uc0{\u803} \uc0{\u273}\uc0{\u244}\uc0{\u769}i v\uc0{\u417}\uc0{\u769}i tr\uc0{\u432}\uc0{\u417}\uc0{\u768}ng d\uc0{\u226}n l\uc0{\u7853}p, t\uc0{\u432} thu\uc0{\u803}c, h\uc0{\u244}\uc0{\u803}i \uc0{\u273}\uc0{\u244}\uc0{\u768}ng thi \uc0{\u273}ua khen th\uc0{\u432}\uc0{\u417}\uc0{\u777}ng v\uc0{\u224} c\uc0{\u225}c h\uc0{\u7897}i \uc0{\u273}\uc0{\u7891}ng kh\uc0{\u225}c).\i0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \i b) C\uc0{\u243} c\uc0{\u225}c t\uc0{\u7893} chuy\uc0{\u234}n m\uc0{\u244}n v\uc0{\u224} t\uc0{\u7893} v\uc0{\u259}n ph\uc0{\u242}ng;\i0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \i c) C\uc0{\u243} t\uc0{\u7893} ch\uc0{\u7913}c \uc0{\u272}\uc0{\u7843}ng C\uc0{\u7897}ng s\uc0{\u7843}n Vi\uc0{\u7879}t Nam, C\uc0{\u244}ng \uc0{\u273}o\uc0{\u224}n, \uc0{\u272}o\uc0{\u224}n Thanh ni\uc0{\u234}n C\uc0{\u7897}ng s\uc0{\u7843}n H\uc0{\u7891} Ch\uc0{\u237} Minh v\uc0{\u224} c\uc0{\u225}c t\uc0{\u7893} ch\uc0{\u7913}c x\uc0{\u227} h\uc0{\u7897}i kh\uc0{\u225}c.\i0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.1.1. M\uc0{\u244} t\uc0{\u7843} hi\uc0{\u7879}n tr\uc0{\u7841}ng}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Tr\uc0{\u432}\uc0{\u7901}ng H\uc0{\u432}\uc0{\u7899}ng D\uc0{\u432}\uc0{\u417}ng hi\uc0{\u7879}n nay c\uc0{\u243} 11 l\uc0{\u7899}p h\uc0{\u7885}c 1 c\uc0{\u417} s\uc0{\u7903} t\uc0{\u7853}p trung, thu\uc0{\u7897}c lo\uc0{\u7841}i tr\uc0{\u432}\uc0{\u7901}ng h\uc0{\u7841}ng I theo qui \uc0{\u273}\uc0{\u7883}nh c\uc0{\u7911}a \uc0{\u273}i\uc0{\u7873}u l\uc0{\u7879} tr\uc0{\u432}\uc0{\u7901}ng MN. Nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng \uc0{\u273}\uc0{\u432}\uc0{\u7907}c ph\uc0{\u226}n b\uc0{\u7893} 01 Hi\uc0{\u7879}u tr\uc0{\u432}\uc0{\u7903}ng \b [H1.1.01.01]; \b0 C\uc0{\u243} 2 ph\uc0{\u243} hi\uc0{\u7879}u tr\uc0{\u432}\uc0{\u7903}ng; 01 ph\uc0{\u243} hi\uc0{\u7879}u tr\uc0{\u432}\uc0{\u7903}ng ph\uc0{\u7909} tr\uc0{\u225}ch v\uc0{\u7873} chuy\uc0{\u234}n m\uc0{\u244}n, 01 ph\uc0{\u243} hi\uc0{\u7879}u tr\uc0{\u432}\uc0{\u7903}ng ph\uc0{\u7909} tr\uc0{\u225}ch c\uc0{\u244}ng t\uc0{\u225}c ch\uc0{\u259}m s\uc0{\u243}c nu\uc0{\u244}i d\uc0{\u432}\uc0{\u7905}ng \b [H1.1.01.02]\b0 ; H\uc0{\u7897}i \uc0{\u273}\uc0{\u7891}ng tr\uc0{\u432}\uc0{\u7901}ng \uc0{\u273}\uc0{\u432}\uc0{\u7907}c th\uc0{\u224}nh l\uc0{\u7853}p n\uc0{\u259}m 2010 g\uc0{\u7891}m 07 th\uc0{\u224}nh vi\uc0{\u234}n theo quy\uc0{\u7871}t \uc0{\u273}\uc0{\u7883}nh \b [H1.1.01.03\b0 ]; C\uc0{\u243} th\uc0{\u224}nh l\uc0{\u7853}p H\uc0{\u7897}i \uc0{\u273}\uc0{\u7891}ng thi \uc0{\u273}ua khen th\uc0{\u432}\uc0{\u7903}ng theo t\uc0{\u7915}ng n\uc0{\u259}m h\uc0{\u7885}c \b [H1.1.01.04]\b0 ; H\uc0{\u7897}i \uc0{\u273}\uc0{\u7891}ng ch\uc0{\u7845}m s\uc0{\u225}ng ki\uc0{\u7871}n kinh nghi\uc0{\u7879}m, ch\uc0{\u7845}m thi gi\uc0{\u225}o vi\uc0{\u234}n gi\uc0{\u7887}i h\uc0{\u224}ng n\uc0{\u259}m \b [H1.1.01.05]\b0 .}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \uc0{\u160}[H1-1-01-01], [H1-1-01-02], [H1-1-01-03], [H1-1-01-04]}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 D\uc0{\u7921}a v\uc0{\u224}o duy\uc0{\u7879}t bi\uc0{\u234}n ch\uc0{\u7871} c\uc0{\u7911}a Ph\uc0{\u242}ng GD&\uc0{\u272}T h\uc0{\u224}ng n\uc0{\u259}m, nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng b\uc0{\u7889} tr\uc0{\u237} gi\uc0{\u225}o vi\uc0{\u234}n, nh\uc0{\u226}n vi\uc0{\u234}n c\uc0{\u243} chuy\uc0{\u234}n m\uc0{\u244}n v\uc0{\u7919}ng v\uc0{\u224}ng, kh\uc0{\u7843} n\uc0{\u259}ng truy\uc0{\u7873}n \uc0{\u273}\uc0{\u7841}t v\uc0{\u224} giao ti\uc0{\u7871}p t\uc0{\u7889}t \uc0{\u273}\uc0{\u7875} \uc0{\u273}\uc0{\u7843}m nh\uc0{\u7853}n c\uc0{\u244}ng t\uc0{\u225}c kh\uc0{\u7889}i tr\uc0{\u432}\uc0{\u7903}ng ph\uc0{\u249} h\uc0{\u7907}p v\uc0{\u7899}i ch\uc0{\u7913}c n\uc0{\u259}ng nhi\uc0{\u7879}m v\uc0{\u7909}, s\uc0{\u7903} tr\uc0{\u432}\uc0{\u7901}ng c\uc0{\u244}ng t\uc0{\u225}c, \b [H1.1.01.08].\b0 [H1-1-01-08]}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Tr\uc0{\u432}\uc0{\u7901}ng\uc0{\u160}th\uc0{\u224}nh l\uc0{\u7853}p chi b\uc0{\u7897} \uc0{\u272}\uc0{\u7843}ng t\uc0{\u7915} n\uc0{\u259}m th\uc0{\u225}ng 3/2012 tr\uc0{\u7921}c thu\uc0{\u7897}c \uc0{\u272}\uc0{\u7843}ng b\uc0{\u7897} x\uc0{\u227} Xu\uc0{\u226}n Th\uc0{\u7841}nh, t\uc0{\u237}nh \uc0{\u273}\uc0{\u7871}n th\uc0{\u7901}i \uc0{\u273}i\uc0{\u7875}m th\uc0{\u225}ng 12/2014 Chi b\uc0{\u7897} c\uc0{\u243} 08 \uc0{\u273}\uc0{\u7843}ng vi\uc0{\u234}n/35 CB-GV- CNV t\uc0{\u7881} l\uc0{\u7879} 22.85% \b [H1.1.01.09]\b0 ; \uc0{\u272}o\uc0{\u224}n Thanh ni\uc0{\u234}n C\uc0{\u7897}ng S\uc0{\u7843}n H\uc0{\u7891} Ch\uc0{\u237} Minh trong nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng c\uc0{\u243} 20 \uc0{\u273}o\uc0{\u224}n vi\uc0{\u234}n, \uc0{\u273}a s\uc0{\u7889} gi\uc0{\u225}o vi\uc0{\u234}n l\uc0{\u224} \uc0{\u273}o\uc0{\u224}n vi\uc0{\u234}n, tr\uc0{\u7867} trung n\uc0{\u259}ng \uc0{\u273}\uc0{\u7897}ng s\uc0{\u225}ng t\uc0{\u7841}o tham gia t\uc0{\u7889}t c\uc0{\u225}c phong tr\uc0{\u224}o c\uc0{\u7911}a tr\uc0{\u432}\uc0{\u7901}ng, ng\uc0{\u224}nh, \uc0{\u273}o\uc0{\u224}n th\uc0{\u7875} \b [H1.1.01.10];\b0  C\uc0{\u244}ng \uc0{\u273}o\uc0{\u224}n \uc0{\u273}\uc0{\u227} c\uc0{\u243} t\uc0{\u7915} khi th\uc0{\u224}nh l\uc0{\u7853}p tr\uc0{\u432}\uc0{\u7901}ng \uc0{\u273}\uc0{\u7871}n th\uc0{\u225}ng 11/2013 c\uc0{\u243} 100% CB-GV-CNV l\uc0{\u224} C\uc0{\u244}ng \uc0{\u273}o\uc0{\u224}n vi\uc0{\u234}n, lu\uc0{\u244}n \uc0{\u273}i \uc0{\u273}\uc0{\u7847}u trong phong tr\uc0{\u224}o thi \uc0{\u273}ua, \uc0{\u273}o\uc0{\u224}n th\uc0{\u7875} ng\uc0{\u224}y c\uc0{\u224}ng v\uc0{\u7919}ng m\uc0{\u7841}nh, \uc0{\u273}\uc0{\u432}\uc0{\u7907}c c\uc0{\u225}c c\uc0{\u7845}p khen th\uc0{\u432}\uc0{\u7903}ng \b [H1.1.01.11].\b0 [H1-1-01-09], [H1-1-01-11]}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.1.2. \uc0{\u272}i\uc0{\u7875}m m\uc0{\u7841}nh}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 BGH tr\uc0{\u7867}, 3/3 \uc0{\u273}\uc0{\u7873}u c\uc0{\u243} tr\uc0{\u236}nh \uc0{\u273}\uc0{\u7897} \uc0{\u273}\uc0{\u7841}i h\uc0{\u7885}c v\uc0{\u224} trung c\uc0{\u7845}p l\uc0{\u253} lu\uc0{\u7853}n ch\uc0{\u237}nh tr\uc0{\u7883}, n\uc0{\u7855}m v\uc0{\u7919}ng chuy\uc0{\u234}n m\uc0{\u244}n nghi\uc0{\u7879}p v\uc0{\u7909}, c\uc0{\u243} n\uc0{\u259}ng l\uc0{\u7921}c trong c\uc0{\u244}ng t\uc0{\u225}c h\uc0{\u224}ng n\uc0{\u259}m \uc0{\u273}\uc0{\u7873}u \uc0{\u273}\uc0{\u7841}t chi\uc0{\u7871}n s\uc0{\u297} thi \uc0{\u273}ua c\uc0{\u7845}p c\uc0{\u417} s\uc0{\u7903} v\uc0{\u224} \uc0{\u273}\uc0{\u432}\uc0{\u7907}c ph\uc0{\u242}ng gi\uc0{\u225}o d\uc0{\u7909}c \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} ho\uc0{\u224}n th\uc0{\u224}nh t\uc0{\u7889}t nhi\uc0{\u7879}m v\uc0{\u7909}.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Tr\uc0{\u432}\uc0{\u7901}ng c\uc0{\u243} \uc0{\u273}\uc0{\u7911} c\uc0{\u225}c t\uc0{\u7893} chuy\uc0{\u234}n m\uc0{\u244}n v\uc0{\u224} t\uc0{\u7893} v\uc0{\u259}n ph\uc0{\u242}ng, c\uc0{\u243} chi b\uc0{\u7897} \uc0{\u272}\uc0{\u7843}ng, \uc0{\u273}o\uc0{\u224}n TNCS.HCM l\uc0{\u224} c\uc0{\u417} s\uc0{\u7903} th\uc0{\u250}c \uc0{\u273}\uc0{\u7849}y c\uc0{\u225}c ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng c\uc0{\u7911}a nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng ho\uc0{\u224}n th\uc0{\u224}nh t\uc0{\u7889}t c\uc0{\u225}c phong tr\uc0{\u224}o trong v\uc0{\u224} ngo\uc0{\u224}i nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng \uc0{\u273}a\uc0{\u771} g\uc0{\u243}p ph\uc0{\u7847}n n\uc0{\u226}ng cao c\uc0{\u244}ng t\uc0{\u225}c ch\uc0{\u259}m s\uc0{\u243}c gi\uc0{\u225}o d\uc0{\u7909}c c\uc0{\u7911}a nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Nhi\uc0{\u7873}u gi\uc0{\u225}o vi\uc0{\u234}n tr\uc0{\u7867} t\uc0{\u225}c phong l\uc0{\u224}m vi\uc0{\u7879}c n\uc0{\u259}ng \uc0{\u273}\uc0{\u7897}ng, s\uc0{\u225}ng t\uc0{\u7841}o tinh th\uc0{\u7847}n tr\uc0{\u225}ch nhi\uc0{\u7879}m, kinh nghi\uc0{\u7879}m trong c\uc0{\u244}ng t\uc0{\u225}c. 100% gi\uc0{\u225}o vi\uc0{\u234}n kh\uc0{\u7889}i L\uc0{\u225} c\uc0{\u243} tr\uc0{\u236}nh \uc0{\u273}\uc0{\u7897} tr\uc0{\u234}n chu\uc0{\u7849}n, n\uc0{\u7855}m v\uc0{\u7919}ng ph\uc0{\u432}\uc0{\u417}ng ph\uc0{\u225}p, chuy\uc0{\u234}n m\uc0{\u244}n trong gi\uc0{\u7843}ng d\uc0{\u7841}y ch\uc0{\u259}m s\uc0{\u243}c tr\uc0{\u7867} c\uc0{\u243} nhi\uc0{\u7873}u kinh nghi\uc0{\u7879}m, th\uc0{\u7921}c hi\uc0{\u7879}n t\uc0{\u7889}t vi\uc0{\u7879}c d\uc0{\u7841}y tr\uc0{\u7867} 5 tu\uc0{\u7893}i.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.1.3. \uc0{\u272}i\uc0{\u7875}m y\uc0{\u7871}u}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Chi b\uc0{\u7897} m\uc0{\u7899}i th\uc0{\u224}nh l\uc0{\u7853}p, \uc0{\u272}\uc0{\u7843}ng vi\uc0{\u234}n \uc0{\u237}t 8 \uc0{\u273}\uc0{\u7843}ng vi\uc0{\u234}n (c\uc0{\u243} 1 d\uc0{\u7921} b\uc0{\u7883}) n\uc0{\u234}n ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng ch\uc0{\u432}a c\uc0{\u243} chi\uc0{\u7873}u s\uc0{\u226}u, th\uc0{\u224}nh t\uc0{\u237}ch ch\uc0{\u432}a c\uc0{\u243}, \uc0{\u273}o\uc0{\u224}n thanh ni\uc0{\u234}n tu\uc0{\u7893}i \uc0{\u273}\uc0{\u7901}i c\uc0{\u242}n non tr\uc0{\u7867}, c\uc0{\u244}ng vi\uc0{\u7879}c c\uc0{\u242}n \uc0{\u273}an xen v\uc0{\u7899}i gi\uc0{\u7843}ng d\uc0{\u7841}y n\uc0{\u234}n phong tr\uc0{\u224}o \uc0{\u273}\uc0{\u7841}t th\uc0{\u224}nh t\uc0{\u237}ch ch\uc0{\u432}a cao.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.1.4. K\uc0{\u7871} ho\uc0{\u7841}ch c\uc0{\u7843}i ti\uc0{\u7871}n ch\uc0{\u7845}t l\uc0{\u432}\uc0{\u7907}ng}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Ph\uc0{\u225}t huy vai tr\uc0{\u242} c\uc0{\u7911}a c\uc0{\u225}c \uc0{\u273}o\uc0{\u224}n th\uc0{\u7875} trong nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng, th\uc0{\u432}\uc0{\u7901}ng xuy\uc0{\u234}n \uc0{\u273}\uc0{\u244}n \uc0{\u273}\uc0{\u7889}c v\uc0{\u224} h\uc0{\u7895} tr\uc0{\u7907} c\uc0{\u225}c phong tr\uc0{\u224}o \uc0{\u273}o\uc0{\u224}n th\uc0{\u7875}, khen th\uc0{\u432}\uc0{\u7903}ng bi\uc0{\u7875}u d\uc0{\u432}\uc0{\u417}ng \uc0{\u273}o\uc0{\u224}n vi\uc0{\u234}n \uc0{\u432}u t\uc0{\u250}. T\uc0{\u7841}o \uc0{\u273}i\uc0{\u7873}u ki\uc0{\u7879}n cho c\uc0{\u225}c \uc0{\u273}o\uc0{\u224}n th\uc0{\u7875} ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng. Duy tr\uc0{\u236} k\uc0{\u7871} ho\uc0{\u7841}ch h\uc0{\u7897}i h\uc0{\u7885}p \uc0{\u273}\uc0{\u7875} r\uc0{\u250}t kinh nghi\uc0{\u7879}m nh\uc0{\u7919}ng vi\uc0{\u7879}c l\uc0{\u224}m ch\uc0{\u432}a \uc0{\u273}\uc0{\u7841}t hi\uc0{\u7879}u qu\uc0{\u7843} cao, \uc0{\u273}\uc0{\u7873} ra bi\uc0{\u7879}n ph\uc0{\u225}p kh\uc0{\u7855}c ph\uc0{\u7909}c}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.1.5. T\uc0{\u7921} \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} ti\uc0{\u234}u ch\uc0{\u237}: \uc0{\u272}\uc0{\u7841}t}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \b \i 1.2. Ti\uc0{\u234}u ch\uc0{\u237} 2: L\uc0{\u417}\uc0{\u769}p ho\uc0{\u803}c, s\uc0{\u7889} tr\uc0{\u7867}, \uc0{\u273}\uc0{\u7883}a \uc0{\u273}i\uc0{\u7875}m tr\uc0{\u432}\uc0{\u7901}ng theo quy \uc0{\u273}\uc0{\u7883}nh c\uc0{\u7911}a \uc0{\u272}i\uc0{\u7873}u l\uc0{\u7879} tr\uc0{\u432}\uc0{\u7901}ng m\uc0{\u7847}m non.\i0 \b0 {\rtlch\fcs1 \af31507 \ltrch\fcs0 \insrsid10420566\charrsid10420566 {\*\bkmkstart tieuchuan0tieuchi1}}}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \i a) L\uc0{\u7899}p h\uc0{\u7885}c \uc0{\u273}\uc0{\u432}\uc0{\u7907}c t\uc0{\u7893} ch\uc0{\u7913}c theo quy \uc0{\u273}\uc0{\u7883}nh;\i0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \i b) S\uc0{\u7889} tr\uc0{\u7867} trong m\uc0{\u7897}t nh\uc0{\u243}m, l\uc0{\u7899}p theo quy \uc0{\u273}\uc0{\u7883}nh;\i0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \i c) \uc0{\u272}\uc0{\u7883}a \uc0{\u273}i\uc0{\u7875}m \uc0{\u273}\uc0{\u7863}t tr\uc0{\u432}\uc0{\u7901}ng, \uc0{\u273}i\uc0{\u7875}m tr\uc0{\u432}\uc0{\u7901}ng theo quy \uc0{\u273}\uc0{\u7883}nh.\i0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.2.1. M\uc0{\u244} t\uc0{\u7843} hi\uc0{\u7879}n tr\uc0{\u7841}ng}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Tr\uc0{\u432}\uc0{\u7901}ng MN H\uc0{\u432}\uc0{\u7899}ng D\uc0{\u432}\uc0{\u417}ng \uc0{\u273}\uc0{\u432}\uc0{\u7907}c th\uc0{\u224}nh l\uc0{\u7853}p theo Q\uc0{\u272} s\uc0{\u7889} 486/Q\uc0{\u272}.UBND Huy\uc0{\u7879}n Th\uc0{\u7889}ng Nh\uc0{\u7845}t. Tr\uc0{\u432}\uc0{\u7901}ng t\uc0{\u7853}p trung t\uc0{\u7841}i m\uc0{\u7897}t \uc0{\u273}i\uc0{\u7875}m, \uc0{\u273}\uc0{\u432}\uc0{\u7907}c \uc0{\u273}\uc0{\u7863}t trong khu trung t\uc0{\u226}m h\uc0{\u224}nh ch\uc0{\u225}nh Huy\uc0{\u7879}n Th\uc0{\u7889}ng nh\uc0{\u7845}t. Thu\uc0{\u7897}c x\uc0{\u227} Xu\uc0{\u226}n Th\uc0{\u7841}nh Huy\uc0{\u7879}n Th\uc0{\u7889}ng Nh\uc0{\u7845}t t\uc0{\u7881}nh \uc0{\u272}\uc0{\u7891}ng Nai \b [H1.1.02.01]; \b0 C\uc0{\u417} s\uc0{\u7903} v\uc0{\u7853}t ch\uc0{\u7845}t t\uc0{\u432}\uc0{\u417}ng \uc0{\u273}\uc0{\u7889}i \uc0{\u273}\uc0{\u7847}y \uc0{\u273}\uc0{\u7911}, t\uc0{\u7841}m th\uc0{\u7901}i \uc0{\u273}\uc0{\u7911} cho tr\uc0{\u7867} ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng khi m\uc0{\u7899}i th\uc0{\u224}nh l\uc0{\u7853}p tr\uc0{\u432}\uc0{\u7901}ng\b [H1.1.02.02]\b0 ; H\uc0{\u7857}ng n\uc0{\u259}m \uc0{\u273}\uc0{\u7873}u c\uc0{\u243} k\uc0{\u7871} ho\uc0{\u7841}ch ph\uc0{\u225}t tri\uc0{\u7875}n, ph\uc0{\u226}n c\uc0{\u244}ng c\uc0{\u7909} th\uc0{\u7875} c\uc0{\u244}ng vi\uc0{\u7879}c cho CB \uc0{\u8211} GV \uc0{\u8211} CNV \uc0{\u273}\uc0{\u250}ng ch\uc0{\u7913}c n\uc0{\u259}ng nhi\uc0{\u7879}m v\uc0{\u7909} \b [H1.1.02.03].\b0 [H1.1.02.01], [H1.1.02.02], [H1.1.02.03]}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 S\uc0{\u7889} nh\uc0{\u243}m l\uc0{\u7899}p v\uc0{\u224} s\uc0{\u7889} tr\uc0{\u7867} \uc0{\u273}\uc0{\u7873}u th\uc0{\u7921}c hi\uc0{\u7879}n theo \uc0{\u273}\uc0{\u250}ng theo \uc0{\u273}i\uc0{\u7873}u l\uc0{\u7879} tr\uc0{\u432}\uc0{\u7901}ng m\uc0{\u7847}m non do b\uc0{\u7897} gi\uc0{\u225}o d\uc0{\u7909}c ban h\uc0{\u224}nh v\uc0{\u224} \uc0{\u273}\uc0{\u432}\uc0{\u7907}c PGD&\uc0{\u272}T ph\uc0{\u234} duy\uc0{\u7879}t \b [H1.1.02.04].\b0 [H1.1.02.04]}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 [H1-1-02-05], [H1-1-02-06], [H1-1-02-07], [H1-1-02-08]S\uc0{\u7889} tr\uc0{\u7867} \uc0{\u273}\uc0{\u432}\uc0{\u7907}c ph\uc0{\u226}n chia \uc0{\u273}\uc0{\u250}ng \uc0{\u273}\uc0{\u7897} tu\uc0{\u7893}i v\uc0{\u7899}i t\uc0{\u7893}ng s\uc0{\u7889} l\uc0{\u7899}p l\uc0{\u224} 11 nh\uc0{\u243}m (13-18) 14/1l\uc0{\u7899}p, (25- 36) 52 tr\uc0{\u7867}/2 l\uc0{\u7899}p, m\uc0{\u7847}m 75tr\uc0{\u7867} /3 l\uc0{\u7899}p, ch\uc0{\u7891}i 75 tr\uc0{\u7867}/ 3 l\uc0{\u7899}p v\uc0{\u224} l\uc0{\u225} 61 tr\uc0{\u7867}/ 2 l\uc0{\u7899}p c\uc0{\u243} h\uc0{\u7891} s\uc0{\u417} v\uc0{\u224} \uc0{\u273}\uc0{\u432}\uc0{\u7907}c l\uc0{\u432}u tr\uc0{\u7919} c\uc0{\u7849}n th\uc0{\u7853}n theo t\uc0{\u7915}ng n\uc0{\u259}m h\uc0{\u7885}c \b [H1.1.02.05\b0 ].[H1-1-02-05]}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.2.2. \uc0{\u272}i\uc0{\u7875}m m\uc0{\u7841}nh}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Tr\uc0{\u432}\uc0{\u7901}ng t\uc0{\u7853}p trung t\uc0{\u7841}i m\uc0{\u7897}t \uc0{\u273}i\uc0{\u7875}m ngay trong khu h\uc0{\u224}nh ch\uc0{\u225}nh Huy\uc0{\u7879}n Th\uc0{\u7889}ng nh\uc0{\u7845}t, \uc0{\u273}\uc0{\u243} l\uc0{\u224} m\uc0{\u7897}t v\uc0{\u7883} tr\uc0{\u237} thu\uc0{\u7853}n l\uc0{\u7907}i, l\uc0{\u224} \uc0{\u273}i\uc0{\u7873}u ki\uc0{\u7879}n t\uc0{\u7889}t cho CMHS \uc0{\u273}\uc0{\u432}a \uc0{\u273}\uc0{\u243}n tr\uc0{\u7867} \uc0{\u273}\uc0{\u7871}n tr\uc0{\u432}\uc0{\u7901}ng, c\uc0{\u417} s\uc0{\u7903} v\uc0{\u7853}t ch\uc0{\u7845}t khang trang s\uc0{\u7841}ch s\uc0{\u7869}, thu\uc0{\u7853}n ti\uc0{\u7879}n trong vi\uc0{\u7879}c \uc0{\u273}\uc0{\u432}a \uc0{\u273}\uc0{\u243}n ch\uc0{\u225}u, \uc0{\u237}t xe c\uc0{\u7897} \uc0{\u7891}n \uc0{\u224}o, an ninh tr\uc0{\u7853}t t\uc0{\u7921} t\uc0{\u7889}t, t\uc0{\u7841}o s\uc0{\u7921} tin t\uc0{\u432}\uc0{\u7903}ng cho ph\uc0{\u7909} huynh an t\uc0{\u226}m g\uc0{\u7917}i tr\uc0{\u7867},v\uc0{\u236} th\uc0{\u7871} \uc0{\u273}\uc0{\u227} thu h\uc0{\u250}t \uc0{\u273}\uc0{\u432}\uc0{\u7907}c nhi\uc0{\u7873}u tr\uc0{\u7867} \uc0{\u273}\uc0{\u7871}n tr\uc0{\u432}\uc0{\u7901}ng, l\uc0{\u7899}p ng\uc0{\u224}y c\uc0{\u224}ng \uc0{\u273}\uc0{\u244}ng.                                                }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1  K\uc0{\u7871} ho\uc0{\u7841}ch ch\uc0{\u7881} ti\uc0{\u234}u ph\uc0{\u225}t tri\uc0{\u7875}n c\uc0{\u7911}a nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng h\uc0{\u7857}ng n\uc0{\u259}m \uc0{\u273}\uc0{\u7873}u v\uc0{\u432}\uc0{\u7907}t k\uc0{\u7871} ho\uc0{\u7841}ch, thu nh\uc0{\u7853}n tr\uc0{\u7867} ph\uc0{\u226}n chia l\uc0{\u7899}p theo \uc0{\u273}\uc0{\u7897} tu\uc0{\u7893}i t\uc0{\u7915} 18 th\uc0{\u225}ng \uc0{\u273}\uc0{\u7871}n 72 th\uc0{\u225}ng. Vi\uc0{\u7879}c ph\uc0{\u226}n c\uc0{\u244}ng , ph\uc0{\u226}n nhi\uc0{\u7879}m cho gi\uc0{\u225}o vi\uc0{\u234}n, nh\uc0{\u226}n vi\uc0{\u234}n ph\uc0{\u249} h\uc0{\u7907}p v\uc0{\u7899}i s\uc0{\u7903} tr\uc0{\u432}\uc0{\u7901}ng, kh\uc0{\u7843} n\uc0{\u259}ng tr\uc0{\u236}nh \uc0{\u273}\uc0{\u7897} chuy\uc0{\u234}n m\uc0{\u244}n v\uc0{\u224} ho\uc0{\u224}n c\uc0{\u7843}nh th\uc0{\u7921}c t\uc0{\u7871} n\uc0{\u234}n t\uc0{\u7845}t c\uc0{\u7843} GV- CNV \uc0{\u273}\uc0{\u7873}u an t\uc0{\u226}m c\uc0{\u244}ng t\uc0{\u225}c. (S\uc0{\u7855}p x\uc0{\u7871}p gi\uc0{\u225}o vi\uc0{\u234}n c\uc0{\u243} kinh nghi\uc0{\u7879}m + 01gi\uc0{\u225}o vi\uc0{\u234}n m\uc0{\u7899}i, gi\uc0{\u225}o vi\uc0{\u234}n c\uc0{\u243} con d\uc0{\u7841}i l\uc0{\u224}m chung v\uc0{\u7899}i gi\uc0{\u225}o vi\uc0{\u234}n \uc0{\u273}\uc0{\u7897}c th\uc0{\u226}n).100% tr\uc0{\u7867} sinh ho\uc0{\u7841}t b\uc0{\u225}n tr\uc0{\u250}, t\uc0{\u7841}o \uc0{\u273}i\uc0{\u7873}u thu\uc0{\u7853}n l\uc0{\u7907}i cho nh\uc0{\u226}n d\uc0{\u226}n lao \uc0{\u273}\uc0{\u7897}ng, CMHS t\uc0{\u237}n nhi\uc0{\u7879}m \uc0{\u273}\uc0{\u432}a con em \uc0{\u273}\uc0{\u7871}n tr\uc0{\u432}\uc0{\u7901}ng ng\uc0{\u224}y c\uc0{\u224}ng \uc0{\u273}\uc0{\u244}ng.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.2.3. \uc0{\u272}i\uc0{\u7875}m y\uc0{\u7871}u}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 S\uc0{\u7889} tr\uc0{\u7867} nh\uc0{\u224} tr\uc0{\u7867} ch\uc0{\u432}a \uc0{\u273}\uc0{\u7841}t ch\uc0{\u7881} ti\uc0{\u234}u m\uc0{\u224} ban ch\uc0{\u7881} \uc0{\u273}\uc0{\u7841}o PC- X\uc0{\u243}a m\uc0{\u249} \uc0{\u273}\uc0{\u7873} ra.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.2.4. K\uc0{\u7871} ho\uc0{\u7841}ch c\uc0{\u7843}i ti\uc0{\u7871}n ch\uc0{\u7845}t l\uc0{\u432}\uc0{\u7907}ng}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Ti\uc0{\u7871}p t\uc0{\u7909}c duy tr\uc0{\u236} k\uc0{\u7871}t qu\uc0{\u7843} \uc0{\u273}\uc0{\u227} \uc0{\u273}\uc0{\u7841}t \uc0{\u273}\uc0{\u432}\uc0{\u7907}c. Th\uc0{\u7921}c hi\uc0{\u7879}n t\uc0{\u7889}t ch\uc0{\u7871} \uc0{\u273}\uc0{\u7897} mi\uc0{\u7877}n, gi\uc0{\u7843}m h\uc0{\u7885}c ph\uc0{\u237} theo Ngh\uc0{\u7883} \uc0{\u273}\uc0{\u7883}nh s\uc0{\u7889} 49/2010/N\uc0{\u272}-CP ng\uc0{\u224}y 14/5/2010. Nh\uc0{\u7857}m thu h\uc0{\u250}t nhi\uc0{\u7873}u tr\uc0{\u7867} \uc0{\u273}\uc0{\u7871}n tr\uc0{\u432}\uc0{\u7901}ng, l\uc0{\u7899}p; T\uc0{\u237}ch c\uc0{\u7921}c tham m\uc0{\u432}u kinh ph\uc0{\u237} mua s\uc0{\u7855}m, tu s\uc0{\u7917}a c\uc0{\u417} s\uc0{\u7903} v\uc0{\u7853}t ch\uc0{\u7845}t, b\uc0{\u7893} sung tr\uc0{\u7891}ng th\uc0{\u234}m hoa, c\uc0{\u226}y ki\uc0{\u7875}ng t\uc0{\u7841}o m\uc0{\u244}i tr\uc0{\u432}\uc0{\u7901}ng lu\uc0{\u244}n Xanh-S\uc0{\u7841}ch - \uc0{\u272}\uc0{\u7865}p t\uc0{\u7841}o m\uc0{\u244}i tr\uc0{\u432}\uc0{\u7901}ng th\uc0{\u226}n thi\uc0{\u7879}n trong v\uc0{\u224} ngo\uc0{\u224}i l\uc0{\u7899}p h\uc0{\u7885}c. }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Ph\uc0{\u226}n c\uc0{\u244}ng gi\uc0{\u225}o vi\uc0{\u234}n \uc0{\u273}i\uc0{\u7873}u tra ph\uc0{\u7893} c\uc0{\u7853}p tr\uc0{\u7867} 5 tu\uc0{\u7893}i tr\uc0{\u234}n \uc0{\u273}\uc0{\u7883}a b\uc0{\u224}n \uc0{\u273}\uc0{\u432}\uc0{\u7907}c ph\uc0{\u226}n c\uc0{\u244}ng \uc0{\u273}\uc0{\u7875} n\uc0{\u7855}m \uc0{\u273}\uc0{\u432}\uc0{\u7907}c s\uc0{\u7889} tr\uc0{\u7867} t\uc0{\u7915} 0 \uc0{\u8211} 5 tu\uc0{\u7893}i, t\uc0{\u7915} \uc0{\u273}\uc0{\u243} x\uc0{\u226}y d\uc0{\u7921}ng k\uc0{\u7871} ho\uc0{\u7841}ch tuy\uc0{\u7875}n sinh cho s\uc0{\u225}t v\uc0{\u7899}i nhu c\uc0{\u7847}u c\uc0{\u7911}a ban ch\uc0{\u7881} \uc0{\u273}\uc0{\u7841}o ph\uc0{\u7893} c\uc0{\u7853}p \uc0{\u273}\uc0{\u7873} ra. Tuy\uc0{\u234}n truy\uc0{\u7873}n v\uc0{\u7853}n \uc0{\u273}\uc0{\u7897}ng tr\uc0{\u7867} ra l\uc0{\u7899}p b\uc0{\u7857}ng nhi\uc0{\u7873}u h\uc0{\u236}nh th\uc0{\u7913}c nh\uc0{\u432} : k\uc0{\u7871}t h\uc0{\u7907}p v\uc0{\u7899}i \uc0{\u273}\uc0{\u224}i ph\uc0{\u225}t thanh, h\uc0{\u7897}i ph\uc0{\u7909} n\uc0{\u7919}, b\uc0{\u259}ng r\uc0{\u244}n\uc0{\u8230}\uc0{\u273}\uc0{\u7875} ph\uc0{\u7909} huynh n\uc0{\u7855}m \uc0{\u273}\uc0{\u432}\uc0{\u7907}c ng\uc0{\u224}y to\uc0{\u224}n d\uc0{\u226}n \uc0{\u273}\uc0{\u432}a tr\uc0{\u7867} \uc0{\u273}\uc0{\u7871}n tr\uc0{\u432}\uc0{\u7901}ng \uc0{\u273}\uc0{\u7841}t ch\uc0{\u7881} ti\uc0{\u234}u tuy\uc0{\u7875}n sinh m\uc0{\u7897}t l\uc0{\u7847}n v\uc0{\u224}o \uc0{\u273}\uc0{\u7847}u n\uc0{\u259}m h\uc0{\u7885}c.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.2.5. T\uc0{\u7921} \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} ti\uc0{\u234}u ch\uc0{\u237}: \uc0{\u272}\uc0{\u7841}t}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \b \i 1.3. Ti\uc0{\u234}u ch\uc0{\u237} 3: C\uc0{\u417} c\uc0{\u7845}u t\uc0{\u7893} ch\uc0{\u7913}c v\uc0{\u224} vi\uc0{\u7879}c th\uc0{\u7921}c hi\uc0{\u7879}n nhi\uc0{\u7879}m v\uc0{\u7909} c\uc0{\u7911}a ca\uc0{\u769}c t\uc0{\u244}\uc0{\u777} chuy\uc0{\u234}n m\uc0{\u244}n, t\uc0{\u7893} v\uc0{\u259}n ph\uc0{\u242}ng theo quy \uc0{\u273}\uc0{\u7883}nh t\uc0{\u7841}i \uc0{\u272}i\uc0{\u7873}u l\uc0{\u7879} tr\uc0{\u432}\uc0{\u7901}ng m\uc0{\u7847}m non.\i0 \b0 {\rtlch\fcs1 \af31507 \ltrch\fcs0 \insrsid10420566\charrsid10420566 {\*\bkmkstart tieuchuan0tieuchi2}}}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \i a) C\uc0{\u243} c\uc0{\u417} c\uc0{\u7845}u t\uc0{\u7893} ch\uc0{\u7913}c theo quy \uc0{\u273}\uc0{\u7883}nh;\i0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \i b) X\uc0{\u226}y d\uc0{\u7921}ng k\uc0{\u7871} ho\uc0{\u7841}ch ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng c\uc0{\u7911}a t\uc0{\u7893} theo tu\uc0{\u7847}n, th\uc0{\u225}ng, h\uc0{\u7885}c k\uc0{\u7923}, n\uc0{\u259}m h\uc0{\u7885}c v\uc0{\u224} th\uc0{\u7921}c hi\uc0{\u7879}n sinh ho\uc0{\u7841}t t\uc0{\u7893} theo quy \uc0{\u273}\uc0{\u7883}nh;\i0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \i c) Th\uc0{\u7921}c hi\uc0{\u7879}n c\uc0{\u225}c nhi\uc0{\u7879}m v\uc0{\u7909} c\uc0{\u7911}a t\uc0{\u7893} theo quy \uc0{\u273}\uc0{\u7883}nh.\i0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.3.1. M\uc0{\u244} t\uc0{\u7843} hi\uc0{\u7879}n tr\uc0{\u7841}ng}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Nha\uc0{\u768} tr\uc0{\u432}\uc0{\u417}\uc0{\u768}ng co\uc0{\u769} c\uc0{\u417} c\uc0{\u7845}u t\uc0{\u7893} ch\uc0{\u7913}c theo quy \uc0{\u273}\uc0{\u7883}nh, t\uc0{\u7893} chuy\uc0{\u234}n m\uc0{\u244}n g\uc0{\u7891}m c\uc0{\u243} gi\uc0{\u225}o vi\uc0{\u234}n v\uc0{\u224} c\uc0{\u7845}p d\uc0{\u432}\uc0{\u7905}ng, t\uc0{\u7893} v\uc0{\u259}n ph\uc0{\u242}ng g\uc0{\u7891}m Ban gi\uc0{\u225}m hi\uc0{\u7879}u, nh\uc0{\u226}n vi\uc0{\u234}n l\uc0{\u224}m c\uc0{\u244}ng t\uc0{\u225}c y t\uc0{\u7871} tr\uc0{\u432}\uc0{\u7901}ng h\uc0{\u7885}c, v\uc0{\u259}n th\uc0{\u432}, k\uc0{\u7871} to\uc0{\u225}n, v\uc0{\u224} c\uc0{\u225}c nh\uc0{\u226}n vi\uc0{\u234}n kh\uc0{\u225}c. M\uc0{\u7895}i t\uc0{\u7893} \uc0{\u273}\uc0{\u7873}u c\uc0{\u243} t\uc0{\u7893} tr\uc0{\u432}\uc0{\u7903}ng v\uc0{\u224} t\uc0{\u7893} ph\uc0{\u243} ph\uc0{\u7909} tr\uc0{\u225}ch ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng c\uc0{\u7911}a t\uc0{\u7893} \b [H1.1.03.01].\b0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 C\uc0{\u225}c t\uc0{\u7893} x\uc0{\u226}y d\uc0{\u7921}ng k\uc0{\u7871} ho\uc0{\u7841}ch hoa\uc0{\u803}t \uc0{\u273}\uc0{\u244}\uc0{\u803}ng theo tu\uc0{\u7847}n, th\uc0{\u225}ng, h\uc0{\u7885}c k\uc0{\u7923}, n\uc0{\u259}m ho\uc0{\u803}c cu\uc0{\u803} th\uc0{\u7875}. C\uc0{\u225}c t\uc0{\u7893} chuy\uc0{\u234}n m\uc0{\u244}n sinh ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7883}nh k\uc0{\u7923} \uc0{\u237}t nh\uc0{\u7845}t hai tu\uc0{\u7847}n m\uc0{\u7897}t l\uc0{\u7847}n v\uc0{\u224} c\uc0{\u225}c sinh ho\uc0{\u7841}t kh\uc0{\u225}c khi c\uc0{\u243} nhu c\uc0{\u7847}u v\uc0{\u7873} c\uc0{\u244}ng vi\uc0{\u7879}c \b [H1.1.03.02].\b0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 C\uc0{\u225}c t\uc0{\u7893} chuy\uc0{\u234}n m\uc0{\u244}n th\uc0{\u7921}c hi\uc0{\u7879}n b\uc0{\u7891}i d\uc0{\u432}\uc0{\u7905}ng chuy\uc0{\u234}n m\uc0{\u244}n, nghi\uc0{\u7879}p v\uc0{\u7909}, ki\uc0{\u7875}m tra, \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} ch\uc0{\u7845}t l\uc0{\u432}\uc0{\u7907}ng, hi\uc0{\u7879}u qu\uc0{\u7843} c\uc0{\u244}ng t\uc0{\u225}c nu\uc0{\u244}i d\uc0{\u432}\uc0{\u7905}ng ch\uc0{\u259}m s\uc0{\u243}c gi\uc0{\u225}o d\uc0{\u7909}c v\uc0{\u224} qu\uc0{\u7843}n l\uc0{\u253} s\uc0{\u7917} d\uc0{\u7909}ng t\uc0{\u224}i l\uc0{\u7883}\uc0{\u234}u, \uc0{\u273}\uc0{\u7891} d\uc0{\u249}ng \uc0{\u273}\uc0{\u7891} ch\uc0{\u417}i, thi\uc0{\u7871}t b\uc0{\u7883} gi\uc0{\u225}o d\uc0{\u7909}c c\uc0{\u7911}a c\uc0{\u225}c th\uc0{\u224}nh vi\uc0{\u234}n trong t\uc0{\u7893} theo k\uc0{\u7871} ho\uc0{\u7841}ch c\uc0{\u7911}a nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng. Tham gia \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} x\uc0{\u7871}p lo\uc0{\u7841}i gi\uc0{\u225}o vi\uc0{\u234}n theo chu\uc0{\u7849}n ngh\uc0{\u7873} nghi\uc0{\u7879}p gi\uc0{\u225}o vi\uc0{\u234}n m\uc0{\u7847}m non theo quy \uc0{\u273}\uc0{\u7883}nh v\uc0{\u224}o cu\uc0{\u7889}i m\uc0{\u7895}i n\uc0{\u259}m h\uc0{\u7885}c v\uc0{\u224} \uc0{\u273}\uc0{\u7873} xu\uc0{\u7845}t khen th\uc0{\u432}\uc0{\u7903}ng c\uc0{\u225}c gi\uc0{\u225}o vi\uc0{\u234}n trong t\uc0{\u7893}. T\uc0{\u7893} v\uc0{\u259}n ph\uc0{\u242}ng gi\uc0{\u250}p hi\uc0{\u7879}u tr\uc0{\u432}\uc0{\u7903}ng qu\uc0{\u7843}n l\uc0{\u253} t\uc0{\u224}i ch\uc0{\u237}nh, t\uc0{\u224}i s\uc0{\u7843}n, l\uc0{\u432}u gi\uc0{\u7919} h\uc0{\u7891} s\uc0{\u417} c\uc0{\u7911}a nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng. Th\uc0{\u7921}c hi\uc0{\u7879}n b\uc0{\u7891}i d\uc0{\u432}\uc0{\u7905}ng chuy\uc0{\u234}n m\uc0{\u244}n nghi\uc0{\u7879}p v\uc0{\u7909}, ki\uc0{\u7875}m tra \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} ch\uc0{\u7845}t l\uc0{\u432}\uc0{\u7907}ng ch\uc0{\u259}m s\uc0{\u243}c gi\uc0{\u225}o d\uc0{\u7909}c theo k\uc0{\u7871} ho\uc0{\u7841}ch c\uc0{\u7911}a nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng, tham gia \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} x\uc0{\u7871}p lo\uc0{\u7841}i c\uc0{\u225}c th\uc0{\u224}nh vi\uc0{\u234}n trong t\uc0{\u7893}\b  [H1.1.03.03].\b0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.3.2. \uc0{\u272}i\uc0{\u7875}m m\uc0{\u7841}nh}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng c\uc0{\u243} c\uc0{\u417} c\uc0{\u7845}u t\uc0{\u7893} ch\uc0{\u7913}c v\uc0{\u224} vi\uc0{\u7879}c th\uc0{\u7921}c hi\uc0{\u7879}n nhi\uc0{\u7879}m v\uc0{\u7909} c\uc0{\u7911}a c\uc0{\u225}c t\uc0{\u7893} chuy\uc0{\u234}n m\uc0{\u244}n, t\uc0{\u7893} v\uc0{\u259}n ph\uc0{\u242}ng theo quy \uc0{\u273}\uc0{\u7883}nh t\uc0{\u7841}i \uc0{\u273}i\uc0{\u7873}u l\uc0{\u7879} tr\uc0{\u432}\uc0{\u7901}ng m\uc0{\u7847}m non. }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 C\uc0{\u225}c t\uc0{\u7893} \uc0{\u273}\uc0{\u7873}u c\uc0{\u243} k\uc0{\u7871} ho\uc0{\u7841}ch ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng th\uc0{\u432}\uc0{\u7901}ng xuy\uc0{\u234}n v\uc0{\u224} th\uc0{\u7921}c hi\uc0{\u7879}n sinh ho\uc0{\u7841}t t\uc0{\u7893} \uc0{\u273}\uc0{\u250}ng theo quy \uc0{\u273}\uc0{\u7883}nh. Co\uc0{\u769} ca\uc0{\u769}c bi\uc0{\u234}\uc0{\u803}n pha\uc0{\u769}p ca\uc0{\u777}i ti\uc0{\u234}\uc0{\u769}n vi\uc0{\u234}\uc0{\u803}c ki\uc0{\u234}\uc0{\u777}m tra, \uc0{\u273}a\uc0{\u769}nh gia\uc0{\u769} ch\uc0{\u226}\uc0{\u769}t l\uc0{\u432}\uc0{\u417}\uc0{\u803}ng ch\uc0{\u259}m so\uc0{\u769}c, gia\uc0{\u769}o du\uc0{\u803}c tre\uc0{\u777}; kha\uc0{\u777}o sa\uc0{\u769}t ho\uc0{\u803}c sinh th\uc0{\u432}\uc0{\u417}\uc0{\u768}ng xuy\uc0{\u234}n. Co\uc0{\u769} \uc0{\u273}u\uc0{\u777} ca\uc0{\u769}c loa\uc0{\u803}i h\uc0{\u244}\uc0{\u768} s\uc0{\u417}; s\uc0{\u244}\uc0{\u777} sa\uc0{\u769}ch theo qui \uc0{\u273}i\uc0{\u803}nh. }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 C\uc0{\u225}c t\uc0{\u7893} kh\uc0{\u7889}i gi\uc0{\u250}p nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng ho\uc0{\u224}n th\uc0{\u224}nh t\uc0{\u7889}t c\uc0{\u225}c nhi\uc0{\u7879}m v\uc0{\u7909} theo k\uc0{\u7871} ho\uc0{\u7841}ch. L\uc0{\u224} \uc0{\u273}\uc0{\u242}n xeo th\uc0{\u250}c \uc0{\u273}\uc0{\u7849}y c\uc0{\u225}c phong tr\uc0{\u224}o thi \uc0{\u273}ua khen th\uc0{\u432}\uc0{\u7903}ng v\uc0{\u224} c\uc0{\u225}c phong tr\uc0{\u224}o TDTT do c\uc0{\u225}c ban ng\uc0{\u224}nh ph\uc0{\u225}t \uc0{\u273}\uc0{\u7897}ng.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.3.3. \uc0{\u272}i\uc0{\u7875}m y\uc0{\u7871}u}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Vi\uc0{\u7879}c lu\uc0{\u226}n chuy\uc0{\u7875}n nh\uc0{\u226}n vi\uc0{\u234}n v\uc0{\u259}n th\uc0{\u432} gi\uc0{\u7919}a n\uc0{\u259}m h\uc0{\u7885}c, nh\uc0{\u226}n vi\uc0{\u234}n y t\uc0{\u7871} xin th\uc0{\u244}i vi\uc0{\u7879}c l\uc0{\u224}m \uc0{\u7843}nh h\uc0{\u432}\uc0{\u7903}ng \uc0{\u273}\uc0{\u7871}n m\uc0{\u7897}t s\uc0{\u7889} c\uc0{\u244}ng vi\uc0{\u7879}c c\uc0{\u7911}a t\uc0{\u7893} h\uc0{\u224}nh ch\uc0{\u225}nh.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.3.4. K\uc0{\u7871} ho\uc0{\u7841}ch c\uc0{\u7843}i ti\uc0{\u7871}n ch\uc0{\u7845}t l\uc0{\u432}\uc0{\u7907}ng}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Ti\uc0{\u234}\uc0{\u769}p tu\uc0{\u803}c ph\uc0{\u225}t huy nh\uc0{\u7919}ng \uc0{\u273}i\uc0{\u7875}m m\uc0{\u7841}nh c\uc0{\u7911}a c\uc0{\u225}c t\uc0{\u7893} chuy\uc0{\u234}n m\uc0{\u244}n v\uc0{\u224} t\uc0{\u7893} v\uc0{\u259}n ph\uc0{\u242}ng.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Kh\uc0{\u7855}c ph\uc0{\u7909}c nh\uc0{\u7919}ng kh\uc0{\u243} kh\uc0{\u259}n hi\uc0{\u7879}n t\uc0{\u7841}i \uc0{\u273}\uc0{\u7875} ho\uc0{\u224}n th\uc0{\u224}nh t\uc0{\u7889}t c\uc0{\u225}c nhi\uc0{\u7879}m v\uc0{\u7909} \uc0{\u273}\uc0{\u7873} ra trong n\uc0{\u259}m h\uc0{\u7885}c.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.3.5. T\uc0{\u7921} \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} ti\uc0{\u234}u ch\uc0{\u237}: \uc0{\u272}\uc0{\u7841}t}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \b \i 1.4. Ti\uc0{\u234}u ch\uc0{\u237} 4: Ch\uc0{\u7845}p h\uc0{\u224}nh ch\uc0{\u7911} tr\uc0{\u432}\uc0{\u417}ng, ch\uc0{\u237}nh s\uc0{\u225}ch c\uc0{\u7911}a \uc0{\u272}\uc0{\u7843}ng, ph\uc0{\u225}p lu\uc0{\u7853}t c\uc0{\u7911}a Nh\uc0{\u224} n\uc0{\u432}\uc0{\u7899}c, s\uc0{\u7921} l\uc0{\u227}nh \uc0{\u273}\uc0{\u7841}o, ch\uc0{\u7881} \uc0{\u273}\uc0{\u7841}o c\uc0{\u7911}a c\uc0{\u7845}p \uc0{\u7911}y \uc0{\u272}\uc0{\u7843}ng, ch\uc0{\u237}nh quy\uc0{\u7873}n \uc0{\u273}\uc0{\u7883}a ph\uc0{\u432}\uc0{\u417}ng v\uc0{\u224} c\uc0{\u417} quan qu\uc0{\u7843}n l\uc0{\u253} gi\uc0{\u225}o d\uc0{\u7909}c c\uc0{\u225}c c\uc0{\u7845}p; b\uc0{\u7843}o \uc0{\u273}\uc0{\u7843}m Quy ch\uc0{\u7871} th\uc0{\u7921}c hi\uc0{\u7879}n d\uc0{\u226}n ch\uc0{\u7911} trong ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng c\uc0{\u7911}a nh\uc0{\u224}  tr\uc0{\u432}\uc0{\u7901}ng.\i0 \b0 {\rtlch\fcs1 \af31507 \ltrch\fcs0 \insrsid10420566\charrsid10420566 {\*\bkmkstart tieuchuan0tieuchi3}}}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \i a) Th\uc0{\u7921}c hi\uc0{\u7879}n c\uc0{\u225}c ch\uc0{\u7881} th\uc0{\u7883}, ngh\uc0{\u7883} quy\uc0{\u7871}t c\uc0{\u7911}a c\uc0{\u7845}p \uc0{\u7911}y \uc0{\u272}\uc0{\u7843}ng, ch\uc0{\u7845}p h\uc0{\u224}nh s\uc0{\u7921} qu\uc0{\u7843}n l\uc0{\u253} h\uc0{\u224}nh ch\uc0{\u237}nh c\uc0{\u7911}a ch\uc0{\u237}nh quy\uc0{\u7873}n \uc0{\u273}\uc0{\u7883}a ph\uc0{\u432}\uc0{\u417}ng, s\uc0{\u7921} ch\uc0{\u7881} \uc0{\u273}\uc0{\u7841}o v\uc0{\u7873} chuy\uc0{\u234}n m\uc0{\u244}n, nghi\uc0{\u7879}p v\uc0{\u7909} c\uc0{\u7911}a c\uc0{\u417} quan qu\uc0{\u7843}n l\uc0{\u253} gi\uc0{\u225}o d\uc0{\u7909}c.\i0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \i b) Th\uc0{\u7921}c hi\uc0{\u7879}n ch\uc0{\u7871} \uc0{\u273}\uc0{\u7897} b\uc0{\u225}o c\uc0{\u225}o \uc0{\u273}\uc0{\u7883}nh k\uc0{\u7923}, b\uc0{\u225}o c\uc0{\u225}o \uc0{\u273}\uc0{\u7897}t xu\uc0{\u7845}t theo quy \uc0{\u273}\uc0{\u7883}nh;\i0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \i c) B\uc0{\u7843}o \uc0{\u273}\uc0{\u7843}m Quy ch\uc0{\u7871} th\uc0{\u7921}c hi\uc0{\u7879}n d\uc0{\u226}n ch\uc0{\u7911} trong ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng c\uc0{\u7911}a nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng.\i0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.4.1. M\uc0{\u244} t\uc0{\u7843} hi\uc0{\u7879}n tr\uc0{\u7841}ng}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 [H1-1-04-01]Nha\uc0{\u768} tr\uc0{\u432}\uc0{\u417}\uc0{\u768}ng co\uc0{\u769} c\uc0{\u417} c\uc0{\u7845}u t\uc0{\u7893} ch\uc0{\u7913}c theo quy \uc0{\u273}\uc0{\u7883}nh, t\uc0{\u7893} chuy\uc0{\u234}n m\uc0{\u244}n g\uc0{\u7891}m c\uc0{\u243} gi\uc0{\u225}o vi\uc0{\u234}n v\uc0{\u224} c\uc0{\u7845}p d\uc0{\u432}\uc0{\u7905}ng, t\uc0{\u7893} v\uc0{\u259}n ph\uc0{\u242}ng g\uc0{\u7891}m Ban gi\uc0{\u225}m hi\uc0{\u7879}u, nh\uc0{\u226}n vi\uc0{\u234}n l\uc0{\u224}m c\uc0{\u244}ng t\uc0{\u225}c y t\uc0{\u7871} tr\uc0{\u432}\uc0{\u7901}ng h\uc0{\u7885}c, v\uc0{\u259}n th\uc0{\u432}, k\uc0{\u7871} to\uc0{\u225}n, v\uc0{\u224} c\uc0{\u225}c nh\uc0{\u226}n vi\uc0{\u234}n kh\uc0{\u225}c. M\uc0{\u7895}i t\uc0{\u7893} \uc0{\u273}\uc0{\u7873}u c\uc0{\u243} t\uc0{\u7893} tr\uc0{\u432}\uc0{\u7903}ng v\uc0{\u224} t\uc0{\u7893} ph\uc0{\u243} ph\uc0{\u7909} tr\uc0{\u225}ch ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng c\uc0{\u7911}a t\uc0{\u7893} \b [H1.1.03.01].\b0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng th\uc0{\u7921}c hi\uc0{\u7879}n t\uc0{\u7889}t c\uc0{\u225}c ch\uc0{\u7881} th\uc0{\u7883}, ngh\uc0{\u7883} quy\uc0{\u7871}t c\uc0{\u7911}a c\uc0{\u7845}p u\uc0{\u7927} \uc0{\u272}\uc0{\u7843}ng, ch\uc0{\u7845}p h\uc0{\u224}nh qu\uc0{\u7843}n l\uc0{\u253} h\uc0{\u224}nh ch\uc0{\u237}nh c\uc0{\u7911}a ch\uc0{\u237}nh quy\uc0{\u7873}n \uc0{\u273}\uc0{\u7883}a ph\uc0{\u432}\uc0{\u417}ng, s\uc0{\u7921} ch\uc0{\u7881} \uc0{\u273}\uc0{\u7841}o v\uc0{\u7873} chuy\uc0{\u234}n m\uc0{\u244}n nghi\uc0{\u7879}p v\uc0{\u7909} c\uc0{\u7911}a c\uc0{\u417} quan qu\uc0{\u7843}n l\uc0{\u253} gi\uc0{\u225}o d\uc0{\u7909}c\b [H1.1.04.01].\b0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 C\uc0{\u225}c t\uc0{\u7893} ch\uc0{\u7913}c \uc0{\u273}o\uc0{\u224}n th\uc0{\u7875} \uc0{\u273}\uc0{\u7843}m b\uc0{\u7843}o th\uc0{\u7921}c hi\uc0{\u7879}n \uc0{\u273}\uc0{\u250}ng quy ch\uc0{\u7871} d\uc0{\u226}n ch\uc0{\u7911} trong ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng c\uc0{\u7911}a nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng \b [H1.1.04.03].\b0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.4.2. \uc0{\u272}i\uc0{\u7875}m m\uc0{\u7841}nh}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng th\uc0{\u7921}c hi\uc0{\u7879}n t\uc0{\u7889}t c\uc0{\u225}c ch\uc0{\u7911} tr\uc0{\u432}\uc0{\u417}ng, ch\uc0{\u237}nh s\uc0{\u225}ch c\uc0{\u7911}a \uc0{\u272}\uc0{\u7843}ng, ph\uc0{\u225}p lu\uc0{\u7853}t c\uc0{\u7911}a nh\uc0{\u224} n\uc0{\u432}\uc0{\u7899}c v\uc0{\u224} c\uc0{\u225}c ch\uc0{\u237}nh s\uc0{\u225}ch c\uc0{\u243} li\uc0{\u234}n quan \uc0{\u273}\uc0{\u7871}n gi\uc0{\u225}o d\uc0{\u7909}c m\uc0{\u7847}m non. }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Lu\uc0{\u244}n ch\uc0{\u7845}p h\uc0{\u224}nh nghi\uc0{\u234}m s\uc0{\u7921} ch\uc0{\u7881} \uc0{\u273}\uc0{\u7841}o, qu\uc0{\u7843}n l\uc0{\u253} c\uc0{\u7911}a c\uc0{\u7845}p \uc0{\u7911}y \uc0{\u272}\uc0{\u7843}ng, ch\uc0{\u237}nh quy\uc0{\u7873}n \uc0{\u273}\uc0{\u7883}a ph\uc0{\u432}\uc0{\u417}ng; s\uc0{\u7921} ch\uc0{\u7881} \uc0{\u273}\uc0{\u7841}o tr\uc0{\u7921}c ti\uc0{\u7871}p v\uc0{\u7873} chuy\uc0{\u234}n m\uc0{\u244}n, nghi\uc0{\u7879}p v\uc0{\u7909} v\uc0{\u224} th\uc0{\u7921}c hi\uc0{\u7879}n ch\uc0{\u7871} \uc0{\u273}\uc0{\u7897} b\uc0{\u225}o c\uc0{\u225}o v\uc0{\u7899}i c\uc0{\u417} quan qu\uc0{\u7843}n l\uc0{\u253} gi\uc0{\u225}o d\uc0{\u7909}c c\uc0{\u7845}p tr\uc0{\u234}n \uc0{\u273}\uc0{\u250}ng th\uc0{\u7901}i gian quy \uc0{\u273}\uc0{\u7883}nh. }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \uc0{\u272}\uc0{\u7843}m b\uc0{\u7843}o th\uc0{\u7921}c hi\uc0{\u7879}n t\uc0{\u7889}t Quy ch\uc0{\u7871} d\uc0{\u226}n ch\uc0{\u7911} trong m\uc0{\u7885}i ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng c\uc0{\u7911}a nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng, kh\uc0{\u244}ng x\uc0{\u7843}y ra \uc0{\u273}\uc0{\u417}n th\uc0{\u432} khi\uc0{\u7871}u n\uc0{\u7841}i t\uc0{\u7889} c\uc0{\u225}o trong nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.4.3. \uc0{\u272}i\uc0{\u7875}m y\uc0{\u7871}u}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Kh\uc0{\u244}ng c\uc0{\u243}.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.4.4. K\uc0{\u7871} ho\uc0{\u7841}ch c\uc0{\u7843}i ti\uc0{\u7871}n ch\uc0{\u7845}t l\uc0{\u432}\uc0{\u7907}ng}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Ti\uc0{\u234}\uc0{\u769}p tu\uc0{\u803}c th\uc0{\u432}\uc0{\u803}c hi\uc0{\u234}\uc0{\u803}n t\uc0{\u244}\uc0{\u769}t c\uc0{\u225}c chu\uc0{\u777} tr\uc0{\u432}\uc0{\u417}ng chi\uc0{\u769}nh sa\uc0{\u769}ch cu\uc0{\u777}a \uc0{\u272}a\uc0{\u777}ng, pha\uc0{\u769}p lu\uc0{\u226}\uc0{\u803}t cu\uc0{\u777}a nha\uc0{\u768} n\uc0{\u432}\uc0{\u417}\uc0{\u769}c, ho\uc0{\u224}n th\uc0{\u224}nh c\uc0{\u225}c b\uc0{\u225}o c\uc0{\u225}o theo quy \uc0{\u273}\uc0{\u7883}nh v\uc0{\u224} ti\uc0{\u7871}p t\uc0{\u7909}c duy tr\uc0{\u236} th\uc0{\u7921}c hi\uc0{\u7879}n t\uc0{\u7889}t quy ch\uc0{\u7871} d\uc0{\u226}n ch\uc0{\u7911} trong nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.4.5. T\uc0{\u7921} \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} ti\uc0{\u234}u ch\uc0{\u237}: \uc0{\u272}\uc0{\u7841}t}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \b \i 1.5. Ti\uc0{\u234}u ch\uc0{\u237} 5: Qu\uc0{\u7843}n l\uc0{\u253} h\uc0{\u224}nh ch\uc0{\u237}nh, th\uc0{\u7921}c hi\uc0{\u7879}n c\uc0{\u225}c phong tr\uc0{\u224}o thi \uc0{\u273}ua theo quy \uc0{\u273}\uc0{\u7883}nh.\i0 \b0 {\rtlch\fcs1 \af31507 \ltrch\fcs0 \insrsid10420566\charrsid10420566 {\*\bkmkstart tieuchuan0tieuchi4}}}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \i a) C\uc0{\u243} \uc0{\u273}\uc0{\u7911} h\uc0{\u7891} s\uc0{\u417} ph\uc0{\u7909}c v\uc0{\u7909} ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng gi\uc0{\u225}o d\uc0{\u7909}c c\uc0{\u7911}a nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng theo quy \uc0{\u273}\uc0{\u7883}nh c\uc0{\u7911}a \uc0{\u272}i\uc0{\u7873}u l\uc0{\u7879} tr\uc0{\u432}\uc0{\u7901}ng m\uc0{\u7847}m non.\i0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \i b) L\uc0{\u432}u tr\uc0{\u7919} \uc0{\u273}\uc0{\u7847}y \uc0{\u273}\uc0{\u7911}, khoa h\uc0{\u7885}c h\uc0{\u7891} s\uc0{\u417}, v\uc0{\u259}n b\uc0{\u7843}n theo quy \uc0{\u273}\uc0{\u7883}nh c\uc0{\u7911}a Lu\uc0{\u7853}t L\uc0{\u432}u tr\uc0{\u7919};\i0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \i c) Th\uc0{\u7921}c hi\uc0{\u7879}n c\uc0{\u225}c cu\uc0{\u7897}c v\uc0{\u7853}n \uc0{\u273}\uc0{\u7897}ng, t\uc0{\u7893} ch\uc0{\u7913}c v\uc0{\u224} duy tr\uc0{\u236} phong tr\uc0{\u224}o thi \uc0{\u273}ua theo h\uc0{\u432}\uc0{\u7899}ng d\uc0{\u7851}n c\uc0{\u7911}a ng\uc0{\u224}nh v\uc0{\u224} quy \uc0{\u273}\uc0{\u7883}nh c\uc0{\u7911}a Nh\uc0{\u224} n\uc0{\u432}\uc0{\u7899}c.\i0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.5.1. M\uc0{\u244} t\uc0{\u7843} hi\uc0{\u7879}n tr\uc0{\u7841}ng}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng c\uc0{\u243} \uc0{\u273}\uc0{\u7847}y \uc0{\u273}\uc0{\u7911} c\uc0{\u225}c h\uc0{\u7891} s\uc0{\u417} theo quy \uc0{\u273}\uc0{\u7883}nh t\uc0{\u7841}i \uc0{\u273}i\uc0{\u7873}u 25 c\uc0{\u7911}a \uc0{\u272}i\uc0{\u7873}u l\uc0{\u7879} tr\uc0{\u432}\uc0{\u417}\uc0{\u768}ng m\uc0{\u226}\uc0{\u768}m non ban h\uc0{\u224}nh t\uc0{\u7841}i v\uc0{\u259}n b\uc0{\u7843}n h\uc0{\u7907}p nh\uc0{\u7845}t s\uc0{\u7889} 05/VBHN \uc0{\u8211} BGD\uc0{\u272}T Ng\uc0{\u224}y 13/02/2014 c\uc0{\u7911}a B\uc0{\u7897} Gi\uc0{\u225}o d\uc0{\u7909}c v\uc0{\u224} \uc0{\u272}\uc0{\u224}o t\uc0{\u7841}o \b [H2.1.05.01].\b0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Nha\uc0{\u768} tr\uc0{\u432}\uc0{\u417}\uc0{\u768}ng th\uc0{\u432}\uc0{\u803}c hi\uc0{\u234}\uc0{\u803}n qua\uc0{\u777}n ly\uc0{\u769}, l\uc0{\u432}u tr\uc0{\u432}\uc0{\u771} h\uc0{\u244}\uc0{\u768} s\uc0{\u417}, v\uc0{\u259}n b\uc0{\u7843}n \uc0{\u273}\uc0{\u226}\uc0{\u768}y \uc0{\u273}u\uc0{\u777} theo quy \uc0{\u273}i\uc0{\u803}nh \b [H2.1.05.02].\b0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng c\uc0{\u243} k\uc0{\u7871} ho\uc0{\u7841}ch th\uc0{\u7921}c hi\uc0{\u7879}n c\uc0{\u225}c cu\uc0{\u7897}c v\uc0{\u7853}n \uc0{\u273}\uc0{\u7897}ng\b .\b0  Th\uc0{\u432}\uc0{\u7901}ng xuy\uc0{\u234}n t\uc0{\u7893} ch\uc0{\u7913}c v\uc0{\u224} duy tr\uc0{\u236} c\uc0{\u225}c phong tr\uc0{\u224}o thi \uc0{\u273}ua theo h\uc0{\u432}\uc0{\u7899}ng d\uc0{\u7851}n c\uc0{\u7911}a ng\uc0{\u224}nh v\uc0{\u224} quy \uc0{\u273}\uc0{\u7883}nh c\uc0{\u7911}a nh\uc0{\u224} n\uc0{\u432}\uc0{\u7899}c. C\uc0{\u243} k\uc0{\u7871} ho\uc0{\u7841}ch th\uc0{\u7921}c hi\uc0{\u7879}n phong tr\uc0{\u224}o thi \uc0{\u273}ua c\uc0{\u7911}a nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng theo t\uc0{\u7915}ng n\uc0{\u259}m h\uc0{\u7885}c. B\uc0{\u225}o c\uc0{\u225}o k\uc0{\u7871}t qu\uc0{\u7843} thi \uc0{\u273}ua c\uc0{\u7911}a nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng cu\uc0{\u7889}i n\uc0{\u259}m h\uc0{\u7885}c. Danh s\uc0{\u225}ch khen th\uc0{\u432}\uc0{\u7903}ng \uc0{\u273}\uc0{\u7897}i ng\uc0{\u361} CB \uc0{\u8211} GV - NV \b [H2.1.05.03], [H2.1.05.04].\b0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.5.2. \uc0{\u272}i\uc0{\u7875}m m\uc0{\u7841}nh}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng c\uc0{\u243} \uc0{\u273}\uc0{\u7847}y \uc0{\u273}\uc0{\u7911} h\uc0{\u7891} s\uc0{\u417} ph\uc0{\u7909}c v\uc0{\u7909} cho ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng c\uc0{\u7911}a nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng theo quy \uc0{\u273}\uc0{\u7883}nh c\uc0{\u7911}a \uc0{\u272}i\uc0{\u7873}u l\uc0{\u7879} tr\uc0{\u432}\uc0{\u7901}ng m\uc0{\u7847}m non. }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 L\uc0{\u432}u tr\uc0{\u7919} \uc0{\u273}\uc0{\u7847}y \uc0{\u273}\uc0{\u7911} h\uc0{\u7891} s\uc0{\u417}, v\uc0{\u259}n b\uc0{\u7843}n theo quy \uc0{\u273}\uc0{\u7883}nh c\uc0{\u7911}a lu\uc0{\u7853}t l\uc0{\u432}u tr\uc0{\u7919}. }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Duy tr\uc0{\u236} v\uc0{\u224} t\uc0{\u7893} ch\uc0{\u7913}c th\uc0{\u7921}c hi\uc0{\u7879}n t\uc0{\u7889}t c\uc0{\u225}c phong tr\uc0{\u224}o thi \uc0{\u273}ua, c\uc0{\u225}c cu\uc0{\u7897}c v\uc0{\u7853}n \uc0{\u273}\uc0{\u7897}ng do ng\uc0{\u224}nh v\uc0{\u224} \uc0{\u273}\uc0{\u7883}a ph\uc0{\u432}\uc0{\u417}ng ph\uc0{\u225}t \uc0{\u273}\uc0{\u7897}ng.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.5.3. \uc0{\u272}i\uc0{\u7875}m y\uc0{\u7871}u}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Hi\uc0{\u7879}n t\uc0{\u7841}i v\uc0{\u259}n th\uc0{\u432} c\uc0{\u7911}a tr\uc0{\u432}\uc0{\u7901}ng \uc0{\u273}\uc0{\u227} \uc0{\u273}\uc0{\u432}\uc0{\u7907}c lu\uc0{\u226}n chuy\uc0{\u7875}n c\uc0{\u244}ng t\uc0{\u225}c theo v\uc0{\u7883} tr\uc0{\u237} vi\uc0{\u7879}c l\uc0{\u224}m( V\uc0{\u236} b\uc0{\u7857}ng c\uc0{\u7845}p k\uc0{\u7871} to\uc0{\u225}n kh\uc0{\u244}ng ph\uc0{\u249} h\uc0{\u7907}p v\uc0{\u7899}i c\uc0{\u244}ng t\uc0{\u225}c v\uc0{\u259}n th\uc0{\u432})}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.5.4. K\uc0{\u7871} ho\uc0{\u7841}ch c\uc0{\u7843}i ti\uc0{\u7871}n ch\uc0{\u7845}t l\uc0{\u432}\uc0{\u7907}ng}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Ti\uc0{\u7871}p t\uc0{\u7909}c th\uc0{\u7921}c hi\uc0{\u234}n c\uc0{\u244}ng t\uc0{\u225}c c\uc0{\u7843}i c\uc0{\u225}ch h\uc0{\u224}nh ch\uc0{\u237}nh v\uc0{\u224} duy tr\uc0{\u236} c\uc0{\u225}c phong tr\uc0{\u224}o thi \uc0{\u273}ua \uc0{\u273}\uc0{\u227} \uc0{\u273}\uc0{\u7841}t \uc0{\u273}\uc0{\u432}\uc0{\u7907}c.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng c\uc0{\u243} k\uc0{\u7871} ho\uc0{\u7841}ch tuy\uc0{\u7875}n d\uc0{\u7909}ng v\uc0{\u259}n th\uc0{\u432} m\uc0{\u7899}i, c\uc0{\u243} b\uc0{\u7857}ng c\uc0{\u7845}p ph\uc0{\u249} h\uc0{\u7907}p \uc0{\u273}\uc0{\u7875} l\uc0{\u224}m t\uc0{\u7889}t c\uc0{\u244}ng t\uc0{\u225}c l\uc0{\u432}u tr\uc0{\u7919} \uc0{\u273}\uc0{\u7841}t hi\uc0{\u7879}u qu\uc0{\u7843} h\uc0{\u417}n.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.5.5. T\uc0{\u7921} \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} ti\uc0{\u234}u ch\uc0{\u237}: \uc0{\u272}\uc0{\u7841}t}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \b \i 1.6. Ti\uc0{\u234}u ch\uc0{\u237} 6: Qu\uc0{\u7843}n l\uc0{\u253} ca\uc0{\u769}c hoa\uc0{\u803}t \uc0{\u273}\uc0{\u244}\uc0{\u803}ng gia\uc0{\u769}o du\uc0{\u803}c, qu\uc0{\u7843}n l\uc0{\u253} c\uc0{\u225}n b\uc0{\u7897}, gi\uc0{\u225}o vi\uc0{\u234}n, nh\uc0{\u226}n vi\uc0{\u234}n, tr\uc0{\u7867} v\uc0{\u224} qu\uc0{\u7843}n l\uc0{\u253} t\uc0{\u224}i ch\uc0{\u237}nh, \uc0{\u273}\uc0{\u7845}t \uc0{\u273}ai, c\uc0{\u417} s\uc0{\u7903} v\uc0{\u7853}t ch\uc0{\u7845}t theo quy \uc0{\u273}\uc0{\u7883}nh.\i0 \b0 {\rtlch\fcs1 \af31507 \ltrch\fcs0 \insrsid10420566\charrsid10420566 {\*\bkmkstart tieuchuan0tieuchi5}}}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \i a) Th\uc0{\u432}\uc0{\u803}c hi\uc0{\u234}\uc0{\u803}n nhi\uc0{\u234}\uc0{\u803}m vu\uc0{\u803} qua\uc0{\u777}n ly\uc0{\u769} ca\uc0{\u769}c hoa\uc0{\u803}t \uc0{\u273}\uc0{\u244}\uc0{\u803}ng gia\uc0{\u769}o du\uc0{\u803}c va\uc0{\u768} qua\uc0{\u777}n ly\uc0{\u769} tr\uc0{\u7867} theo quy \uc0{\u273}\uc0{\u7883}nh c\uc0{\u7911}a \uc0{\u272}i\uc0{\u7873}u l\uc0{\u7879} tr\uc0{\u432}\uc0{\u7901}ng m\uc0{\u7847}m non.\i0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \i b) Th\uc0{\u7921}c hi\uc0{\u7879}n tuy\uc0{\u7875}n d\uc0{\u7909}ng, \uc0{\u273}\uc0{\u7873} b\uc0{\u7841}t, b\uc0{\u7893} nhi\uc0{\u7879}m, qu\uc0{\u7843}n l\uc0{\u253} c\uc0{\u225}n b\uc0{\u7897}, gi\uc0{\u225}o vi\uc0{\u234}n v\uc0{\u224} nh\uc0{\u226}n vi\uc0{\u234}n theo quy \uc0{\u273}\uc0{\u7883}nh c\uc0{\u7911}a Lu\uc0{\u7853}t C\uc0{\u225}n b\uc0{\u7897}, c\uc0{\u244}ng ch\uc0{\u7913}c, Lu\uc0{\u7853}t Vi\uc0{\u234}n ch\uc0{\u7913}c, \uc0{\u272}i\uc0{\u7873}u l\uc0{\u7879} tr\uc0{\u432}\uc0{\u7901}ng m\uc0{\u7847}m non v\uc0{\u224} c\uc0{\u225}c quy \uc0{\u273}\uc0{\u7883}nh kh\uc0{\u225}c c\uc0{\u7911}a ph\uc0{\u225}p lu\uc0{\u7853}t.\i0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \i c) Qu\uc0{\u7843}n l\uc0{\u253}, s\uc0{\u7917} d\uc0{\u7909}ng hi\uc0{\u7879}u qu\uc0{\u7843} t\uc0{\u224}i ch\uc0{\u237}nh, \uc0{\u273}\uc0{\u7845}t \uc0{\u273}ai, c\uc0{\u417} s\uc0{\u7903} v\uc0{\u7853}t ch\uc0{\u7845}t \uc0{\u273}\uc0{\u7875} ph\uc0{\u7909}c v\uc0{\u7909} c\uc0{\u225}c ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng gi\uc0{\u225}o d\uc0{\u7909}c.\i0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.6.1. M\uc0{\u244} t\uc0{\u7843} hi\uc0{\u7879}n tr\uc0{\u7841}ng}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng th\uc0{\u7921}c hi\uc0{\u7879}n nhi\uc0{\u7879}m v\uc0{\u7909} qu\uc0{\u7843}n l\uc0{\u253} c\uc0{\u225}c ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng gi\uc0{\u225}o d\uc0{\u7909}c v\uc0{\u224} qu\uc0{\u7843}n l\uc0{\u253} tr\uc0{\u7867} theo quy \uc0{\u273}\uc0{\u7883}nh c\uc0{\u7911}a \uc0{\u272}i\uc0{\u7873}u l\uc0{\u7879} tr\uc0{\u432}\uc0{\u7901}ng m\uc0{\u7847}m non. X\uc0{\u226}y d\uc0{\u7921}ng k\uc0{\u7871} ho\uc0{\u7841}ch v\uc0{\u224} b\uc0{\u225}o c\uc0{\u225}o c\uc0{\u225}c ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng gi\uc0{\u225}o d\uc0{\u7909}c theo quy \uc0{\u273}\uc0{\u7883}nh \b [H2.1.06.01].\b0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 H\uc0{\u224}ng n\uc0{\u259}m nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng th\uc0{\u7921}c hi\uc0{\u7879}n tuy\uc0{\u7875}n d\uc0{\u7909}ng, \uc0{\u273}\uc0{\u7873} b\uc0{\u7841}t, b\uc0{\u7893} nhi\uc0{\u7879}m, qui ho\uc0{\u7841}ch CBQL theo \uc0{\u273}\uc0{\u250}ng quy tr\uc0{\u236}nh, quy \uc0{\u273}\uc0{\u7883}nh c\uc0{\u7911}a Lu\uc0{\u7853}t C\uc0{\u225}n b\uc0{\u7897}, c\uc0{\u244}ng ch\uc0{\u7913}c, Lu\uc0{\u7853}t Vi\uc0{\u234}n ch\uc0{\u7913}c. \uc0{\u272}i\uc0{\u7873}u l\uc0{\u7879} tr\uc0{\u432}\uc0{\u7901}ng m\uc0{\u7847}m non v\uc0{\u224} c\uc0{\u225}c quy \uc0{\u273}\uc0{\u7883}nh kh\uc0{\u225}c c\uc0{\u7911}a ph\uc0{\u225}p lu\uc0{\u7853}t. Qu\uc0{\u7843}n l\uc0{\u253} c\uc0{\u225}n b\uc0{\u7897}, gi\uc0{\u225}o vi\uc0{\u234}n v\uc0{\u224} nh\uc0{\u226}n vi\uc0{\u234}n theo quy \uc0{\u273}\uc0{\u7883}nh v\uc0{\u224} c\uc0{\u243} \uc0{\u273}\uc0{\u7847}y \uc0{\u273}\uc0{\u7911} h\uc0{\u7891} s\uc0{\u417} qu\uc0{\u7843}n l\uc0{\u253} nh\uc0{\u226}n s\uc0{\u7921} \b [H2.1.06.02].\b0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng qu\uc0{\u7843}n l\uc0{\u253}, s\uc0{\u7917} d\uc0{\u7909}ng hi\uc0{\u7879}u qu\uc0{\u7843} t\uc0{\u224}i ch\uc0{\u237}nh, \uc0{\u273}\uc0{\u7845}t \uc0{\u273}ai, c\uc0{\u417} s\uc0{\u7903} v\uc0{\u7853}t ch\uc0{\u7845}t \uc0{\u273}\uc0{\u7875} ph\uc0{\u7909}c v\uc0{\u7909} c\uc0{\u225}c ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng gi\uc0{\u225}o d\uc0{\u7909}c. H\uc0{\u224}ng n\uc0{\u259}m c\uc0{\u243} ki\uc0{\u7875}m k\uc0{\u234} t\uc0{\u224}i s\uc0{\u7843}n \uc0{\u273}\uc0{\u7875} theo d\uc0{\u245}i v\uc0{\u224} trang b\uc0{\u7883} b\uc0{\u7893} sung c\uc0{\u225}c trang thi\uc0{\u7871}t b\uc0{\u7883} \uc0{\u273}\uc0{\u7875} ph\uc0{\u7909}c v\uc0{\u7909} t\uc0{\u7889}t h\uc0{\u417}n cho c\uc0{\u225}c ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng gi\uc0{\u225}o d\uc0{\u7909}c \b [H2.1.06.03].\b0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.6.2. \uc0{\u272}i\uc0{\u7875}m m\uc0{\u7841}nh}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng th\uc0{\u7921}c hi\uc0{\u7879}n t\uc0{\u7889}t nhi\uc0{\u7879}m v\uc0{\u7909} qu\uc0{\u7843}n l\uc0{\u253} c\uc0{\u225}c ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng gi\uc0{\u225}o d\uc0{\u7909}c v\uc0{\u224} qu\uc0{\u7843}n l\uc0{\u253} tr\uc0{\u7867} theo quy \uc0{\u273}\uc0{\u7883}nh c\uc0{\u7911}a \uc0{\u272}i\uc0{\u7873}u l\uc0{\u7879} tr\uc0{\u432}\uc0{\u7901}ng m\uc0{\u7847}m non. }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Th\uc0{\u7921}c hi\uc0{\u7879}n tuy\uc0{\u7875}n d\uc0{\u7909}ng, \uc0{\u273}\uc0{\u7873} b\uc0{\u7841}t, b\uc0{\u7893} nhi\uc0{\u7879}m theo \uc0{\u273}\uc0{\u250}ng quy tr\uc0{\u236}nh, quy \uc0{\u273}\uc0{\u7883}nh c\uc0{\u7911}a Lu\uc0{\u7853}t C\uc0{\u225}n b\uc0{\u7897}, c\uc0{\u244}ng ch\uc0{\u7913}c, Lu\uc0{\u7853}t Vi\uc0{\u234}n ch\uc0{\u7913}c, \uc0{\u272}i\uc0{\u7873}u l\uc0{\u7879} tr\uc0{\u432}\uc0{\u7901}ng m\uc0{\u7847}m non v\uc0{\u224} c\uc0{\u225}c quy \uc0{\u273}\uc0{\u7883}nh kh\uc0{\u225}c c\uc0{\u7911}a ph\uc0{\u225}p lu\uc0{\u7853}t.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1  Qu\uc0{\u7843}n l\uc0{\u253}, s\uc0{\u7917} d\uc0{\u7909}ng hi\uc0{\u7879}u qu\uc0{\u7843} t\uc0{\u224}i ch\uc0{\u237}nh, \uc0{\u273}\uc0{\u7845}t \uc0{\u273}ai, c\uc0{\u417} s\uc0{\u7903} v\uc0{\u7853}t ch\uc0{\u7845}t \uc0{\u273}\uc0{\u7875} ph\uc0{\u7909}c v\uc0{\u7909} c\uc0{\u225}c ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng gi\uc0{\u225}o d\uc0{\u7909}c.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.6.3. \uc0{\u272}i\uc0{\u7875}m y\uc0{\u7871}u}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 M\uc0{\u7897}t s\uc0{\u7889} l\uc0{\u7899}p ch\uc0{\u432}a \uc0{\u253} th\uc0{\u7913}c cao trong vi\uc0{\u7879}c b\uc0{\u7843}o v\uc0{\u7879}, gi\uc0{\u7919} g\uc0{\u236}n  t\uc0{\u224}i s\uc0{\u7843}n chung c\uc0{\u7911}a tr\uc0{\u432}\uc0{\u7901}ng, l\uc0{\u7899}p.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.6.4. K\uc0{\u7871} ho\uc0{\u7841}ch c\uc0{\u7843}i ti\uc0{\u7871}n ch\uc0{\u7845}t l\uc0{\u432}\uc0{\u7907}ng}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Ti\uc0{\u234}\uc0{\u769}p tu\uc0{\u803}c th\uc0{\u432}\uc0{\u803}c hi\uc0{\u234}\uc0{\u803}n vi\uc0{\u7879}c c\uc0{\u7843}i c\uc0{\u225}ch h\uc0{\u224}nh ch\uc0{\u237}nh \uc0{\u273}\uc0{\u7875} n\uc0{\u226}ng cao hi\uc0{\u7879}u qu\uc0{\u7843} c\uc0{\u244}ng t\uc0{\u225}c qu\uc0{\u7843}n l\uc0{\u253} v\uc0{\u224} c\uc0{\u225}c ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng gi\uc0{\u225}o d\uc0{\u7909}c, qu\uc0{\u7843}n l\uc0{\u253} c\uc0{\u225}n b\uc0{\u7897} gi\uc0{\u225}o vi\uc0{\u234}n, nh\uc0{\u226}n vi\uc0{\u234}n v\uc0{\u224} tr\uc0{\u7867}. Qu\uc0{\u7843}n l\uc0{\u253} t\uc0{\u224}i ch\uc0{\u237}nh, \uc0{\u273}\uc0{\u7845}t \uc0{\u273}ai, c\uc0{\u417} s\uc0{\u7903} v\uc0{\u7853}t ch\uc0{\u7845}t theo quy \uc0{\u273}\uc0{\u7883}nh. }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Th\uc0{\u432}\uc0{\u7901}ng xuy\uc0{\u234}n ki\uc0{\u7875}m tra vi\uc0{\u7879}c b\uc0{\u7843}o v\uc0{\u7879} t\uc0{\u224}i s\uc0{\u7843}n l\uc0{\u7899}p. \uc0{\u272}\uc0{\u7875} c\uc0{\u243} bi\uc0{\u7879}n ph\uc0{\u225}p \uc0{\u273}\uc0{\u7889}i v\uc0{\u7899}i c\uc0{\u225}c l\uc0{\u7899}p ch\uc0{\u432}a \uc0{\u253} th\uc0{\u7913}c trong vi\uc0{\u7879}c b\uc0{\u7843}o qu\uc0{\u7843}n t\uc0{\u224}i s\uc0{\u7843}n chung.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.6.5. T\uc0{\u7921} \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} ti\uc0{\u234}u ch\uc0{\u237}: \uc0{\u272}\uc0{\u7841}t}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \b \i 1.7. Ti\uc0{\u234}u ch\uc0{\u237} 7: B\uc0{\u7843}o \uc0{\u273}\uc0{\u7843}m an ninh tr\uc0{\u7853}t t\uc0{\u7921}, an to\uc0{\u224}n cho tr\uc0{\u7867} v\uc0{\u224} cho c\uc0{\u225}n b\uc0{\u7897}, gi\uc0{\u225}o vi\uc0{\u234}n, nh\uc0{\u226}n vi\uc0{\u234}n.\i0 \b0 {\rtlch\fcs1 \af31507 \ltrch\fcs0 \insrsid10420566\charrsid10420566 {\*\bkmkstart tieuchuan0tieuchi6}}}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \i a) C\uc0{\u243} ph\uc0{\u432}\uc0{\u417}ng \uc0{\u225}n c\uc0{\u7909} th\uc0{\u7875} b\uc0{\u7843}o \uc0{\u273}\uc0{\u7843}m an ninh tr\uc0{\u7853}t t\uc0{\u7921} trong nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng.\i0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \i b) C\uc0{\u243} ph\uc0{\u432}\uc0{\u417}ng \uc0{\u225}n c\uc0{\u7909} th\uc0{\u7875} ph\uc0{\u242}ng ch\uc0{\u7889}ng tai n\uc0{\u7841}n th\uc0{\u432}\uc0{\u417}ng t\uc0{\u237}ch, ph\uc0{\u242}ng ch\uc0{\u7889}ng ch\uc0{\u225}y n\uc0{\u7893}; ph\uc0{\u242}ng ch\uc0{\u7889}ng d\uc0{\u7883}ch b\uc0{\u7879}nh, ng\uc0{\u7897} \uc0{\u273}\uc0{\u7897}c th\uc0{\u7921}c ph\uc0{\u7849}m trong nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng.\i0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \i c) B\uc0{\u7843}o \uc0{\u273}\uc0{\u7843}m an to\uc0{\u224}n tuy\uc0{\u7879}t \uc0{\u273}\uc0{\u7889}i cho tr\uc0{\u7867} v\uc0{\u224} cho c\uc0{\u225}n b\uc0{\u7897} gi\uc0{\u225}o vi\uc0{\u234}n, nh\uc0{\u226}n vi\uc0{\u234}n trong ph\uc0{\u7841}m vi nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng.\i0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.7.1. M\uc0{\u244} t\uc0{\u7843} hi\uc0{\u7879}n tr\uc0{\u7841}ng}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 H\uc0{\u7857}ng n\uc0{\u259}m nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng X\uc0{\u226}y d\uc0{\u7921}ng k\uc0{\u7871} ho\uc0{\u7841}ch ANTT v\uc0{\u224} ph\uc0{\u7889}i h\uc0{\u7907}p v\uc0{\u7899}i c\uc0{\u244}ng an \uc0{\u273}\uc0{\u7883}a ph\uc0{\u432}\uc0{\u417}ng trong c\uc0{\u244}ng t\uc0{\u225}c ph\uc0{\u242}ng ch\uc0{\u7889}ng t\uc0{\u7897}i ph\uc0{\u7841}m, tuy\uc0{\u234}n truy\uc0{\u7873}n qu\uc0{\u225}n tri\uc0{\u7879}t gi\uc0{\u225}o d\uc0{\u7909}c Ph\uc0{\u225}p lu\uc0{\u7853}t, ch\uc0{\u237}nh tr\uc0{\u7883}, \uc0{\u273}\uc0{\u7841}o \uc0{\u273}\uc0{\u7913}c, l\uc0{\u7889}i s\uc0{\u7889}ng nh\uc0{\u7857}m n\uc0{\u226}ng cao \uc0{\u253} th\uc0{\u7913}c c\uc0{\u7843}nh gi\uc0{\u225}c cho CB-GV-CNV, cha m\uc0{\u7865} h\uc0{\u7885}c sinh, ng\uc0{\u432}\uc0{\u7901}i d\uc0{\u226}n \uc0{\u273}\uc0{\u7883}a ph\uc0{\u432}\uc0{\u417}ng, ph\uc0{\u242}ng, ch\uc0{\u7889}ng t\uc0{\u7897}i ph\uc0{\u7841}m va\uc0{\u768} c\uc0{\u225}c t\uc0{\u234}\uc0{\u803} na\uc0{\u803}n xa\uc0{\u771} h\uc0{\u244}\uc0{\u803}i nh\uc0{\u7857}m \uc0{\u273}\uc0{\u7843}m b\uc0{\u7843}o an ninh, tr\uc0{\u7853}t t\uc0{\u7921} trong nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng. \b [H1.1.07.01].\b0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 X\uc0{\u226}y d\uc0{\u7921}ng k\uc0{\u7871} ho\uc0{\u7841}ch Tr\uc0{\u432}\uc0{\u7901}ng h\uc0{\u7885}c AT-PCCTTT trong nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng \b [H1.1.07.02];\b0 C\uc0{\u243} k\uc0{\u7871} ho\uc0{\u7841}ch PCCC \uc0{\u273}\uc0{\u7843}m b\uc0{\u7843}o tuy\uc0{\u7879}t \uc0{\u273}\uc0{\u7889}i khi c\uc0{\u243} s\uc0{\u7921} c\uc0{\u7889} s\uc0{\u7843}y ra \b [H1.1.07.03]\b0 ; X\uc0{\u226}y d\uc0{\u7921}ng KH v\uc0{\u224} ti\uc0{\u234}m ng\uc0{\u7915}a m\uc0{\u7897}t s\uc0{\u7889} d\uc0{\u7883}ch b\uc0{\u7879}nh cho tr\uc0{\u7867}, tuy\uc0{\u234}n truy\uc0{\u7873}n m\uc0{\u7897}t s\uc0{\u7889} b\uc0{\u224}i vi\uc0{\u7871}t, tranh \uc0{\u7843}nh v\uc0{\u7873} ng\uc0{\u7897} \uc0{\u273}\uc0{\u7897}c th\uc0{\u7921}c ph\uc0{\u7849}m th\uc0{\u7921}c ph\uc0{\u7849}m s\uc0{\u7843}y ra trong nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng nh\uc0{\u7857}m h\uc0{\u7841}n ch\uc0{\u7871} t\uc0{\u7889}i \uc0{\u273}a vi\uc0{\u7879}c ng\uc0{\u7897} \uc0{\u273}\uc0{\u7897}c th\uc0{\u7921}c ph\uc0{\u7849}m h\uc0{\u224}ng ng\uc0{\u224}y cho tr\uc0{\u7867}\b [H1.1.07.04].\b0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Tr\uc0{\u432}\uc0{\u7901}ng c\uc0{\u243} ph\uc0{\u226}n c\uc0{\u244}ng c\uc0{\u7909} th\uc0{\u7875} l\uc0{\u7883}ch tr\uc0{\u7921}c b\uc0{\u7843}o v\uc0{\u7879} 24/24. \uc0{\u272}\uc0{\u7843}m b\uc0{\u7843}o tuy\uc0{\u7879}t \uc0{\u273}\uc0{\u7889}i cho ng\uc0{\u432}\uc0{\u7901}i v\uc0{\u224} t\uc0{\u224}i s\uc0{\u7843}n cho tr\uc0{\u7867} v\uc0{\u224} CB \uc0{\u8211} GV \uc0{\u8211}CNV trong th\uc0{\u7901}i gian \uc0{\u7903} tr\uc0{\u432}\uc0{\u7901}ng \b [H1.1.07.05].\b0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.7.2. \uc0{\u272}i\uc0{\u7875}m m\uc0{\u7841}nh}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng ph\uc0{\u7889}i h\uc0{\u7907}p t\uc0{\u7889}t v\uc0{\u7899}i l\uc0{\u7921}c l\uc0{\u432}\uc0{\u7907}ng an ninh c\uc0{\u7911}a \uc0{\u273}\uc0{\u7883}a ph\uc0{\u432}\uc0{\u417}ng n\uc0{\u234}n h\uc0{\u224}ng n\uc0{\u259}m kh\uc0{\u244}ng c\uc0{\u243} hi\uc0{\u7879}n t\uc0{\u432}\uc0{\u7907}ng qu\uc0{\u7851}y nhi\uc0{\u7877}u, g\uc0{\u226}y m\uc0{\u7845}t tr\uc0{\u7853}t t\uc0{\u7921} c\uc0{\u244}ng c\uc0{\u244}ng c\uc0{\u7897}ng ho\uc0{\u7863}c tr\uc0{\u244}m c\uc0{\u7855}p s\uc0{\u7843}y ra.C\uc0{\u7855}t c\uc0{\u7917} b\uc0{\u7843}o v\uc0{\u7879} tr\uc0{\u7921}c 24/24 trong v\uc0{\u224} ngo\uc0{\u224}i gi\uc0{\u7901} h\uc0{\u7885}c \uc0{\u273}\uc0{\u7843}m b\uc0{\u7843}o an to\uc0{\u224}n tuy\uc0{\u7879}t \uc0{\u273}\uc0{\u7889}i cho tr\uc0{\u7867},c\uc0{\u243} l\uc0{\u7883}ch tr\uc0{\u7921}c GV v\uc0{\u224} l\uc0{\u227}nh \uc0{\u273}\uc0{\u7841}o trong nh\uc0{\u7919}ng ng\uc0{\u224}y l\uc0{\u7877}, t\uc0{\u7871}t, h\uc0{\u232}. C\uc0{\u243} gi\uc0{\u225}m s\uc0{\u225}t c\uc0{\u7911}a BGH v\uc0{\u236} th\uc0{\u7871} kh\uc0{\u244}ng c\uc0{\u243} tr\uc0{\u432}\uc0{\u7901}ng h\uc0{\u7907}p b\uc0{\u7845}t tr\uc0{\u7855}c s\uc0{\u7843}y ra trong nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng. }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 C\uc0{\u243} k\uc0{\u7871} ho\uc0{\u7841}ch cho CB -  GV \uc0{\u8211} CNV \uc0{\u273}i t\uc0{\u7853}p hu\uc0{\u7845}n PCCC.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Th\uc0{\u7921}c hi\uc0{\u7879}n t\uc0{\u7889}t k\uc0{\u7871} ho\uc0{\u7841}ch ph\uc0{\u242}ng, ch\uc0{\u7889}ng tai n\uc0{\u7841}n th\uc0{\u432}\uc0{\u417}ng t\uc0{\u237}ch, ph\uc0{\u242}ng b\uc0{\u7879}nh trong nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng; tuy\uc0{\u234}n truy\uc0{\u7873}n, \uc0{\u273}\uc0{\u7843}m b\uc0{\u7843}o kh\uc0{\u226}u v\uc0{\u7879} sinh an to\uc0{\u224}n th\uc0{\u7921}c ph\uc0{\u7849}m.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \uc0{\u272}\uc0{\u7843}m b\uc0{\u7843}o an to\uc0{\u224}n tuy\uc0{\u7879}t \uc0{\u273}\uc0{\u7889}i cho CB \uc0{\u8211}GV \uc0{\u8211}CNV v\uc0{\u224} tr\uc0{\u7867} trong th\uc0{\u7901}i gian \uc0{\u7903} tr\uc0{\u432}\uc0{\u7901}ng.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.7.3. \uc0{\u272}i\uc0{\u7875}m y\uc0{\u7871}u}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 B\uc0{\u7843}o v\uc0{\u7879} thay \uc0{\u273}\uc0{\u7893}i th\uc0{\u432}\uc0{\u7901}ng xuy\uc0{\u234}n n\uc0{\u234}n ch\uc0{\u432}a c\uc0{\u243} nghi\uc0{\u7879}p v\uc0{\u7909} b\uc0{\u7843}o v\uc0{\u7879}. Do kinh ph\uc0{\u237} d\uc0{\u7921} to\uc0{\u225}n c\uc0{\u242}n h\uc0{\u7841}n h\uc0{\u7865}p n\uc0{\u234}n ch\uc0{\u432}a trang b\uc0{\u7883} th\uc0{\u234}m d\uc0{\u7909}ng c\uc0{\u7909} ch\uc0{\u7919}a ch\uc0{\u225}y v\uc0{\u224} ph\uc0{\u432}\uc0{\u417}ng ti\uc0{\u7879}n h\uc0{\u7895} tr\uc0{\u7907} ch\uc0{\u7919}a ch\uc0{\u225}y}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.7.4. K\uc0{\u7871} ho\uc0{\u7841}ch c\uc0{\u7843}i ti\uc0{\u7871}n ch\uc0{\u7845}t l\uc0{\u432}\uc0{\u7907}ng}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Duy tr\uc0{\u236} \uc0{\u273}\uc0{\u7897}i PCCC trong nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng, h\uc0{\u7857}ng n\uc0{\u259}m tr\uc0{\u432}\uc0{\u7901}ng mua b\uc0{\u7893} sung b\uc0{\u236}nh kh\uc0{\u237} Co2; li\uc0{\u234}n h\uc0{\u7879} \uc0{\u273}\uc0{\u7897}i PCCC huy\uc0{\u7879}n \uc0{\u273}\uc0{\u7875} b\uc0{\u7843}o d\uc0{\u432}\uc0{\u7905}ng \uc0{\u273}\uc0{\u7883}nh k\uc0{\u7923} b\uc0{\u236}nh b\uc0{\u7897}t, trang b\uc0{\u7883} b\uc0{\u7897} \uc0{\u273}\uc0{\u232}n ch\uc0{\u7881} d\uc0{\u7851}n l\uc0{\u7889}i tho\uc0{\u225}t hi\uc0{\u7875}m \uc0{\u7903} c\uc0{\u7847}u thang, (kinh ph\uc0{\u237} trang b\uc0{\u7883} d\uc0{\u7909}ng c\uc0{\u7909} PCCC t\uc0{\u7915} ngu\uc0{\u7891}n ng\uc0{\u226}n s\uc0{\u225}ch). \uc0{\u272}\uc0{\u7897}i PCCC nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng h\uc0{\u432}\uc0{\u7899}ng d\uc0{\u7851}n cho GV-CNV th\uc0{\u7921}c hi\uc0{\u7879}n c\uc0{\u225}ch s\uc0{\u7917} d\uc0{\u7909}ng b\uc0{\u236}nh ch\uc0{\u7919}a ch\uc0{\u225}y, t\uc0{\u7853}p d\uc0{\u7907}t t\uc0{\u236}nh hu\uc0{\u7889}ng gi\uc0{\u7843} \uc0{\u273}\uc0{\u7883}nh cho tr\uc0{\u7867} 02 l\uc0{\u7847}n/n\uc0{\u259}m \uc0{\u273}\uc0{\u7875} ph\uc0{\u242}ng khi c\uc0{\u243} s\uc0{\u7921} c\uc0{\u7889} x\uc0{\u7843}y ra. Ngo\uc0{\u224}i ra \uc0{\u273}\uc0{\u7897}i PCCC tr\uc0{\u432}\uc0{\u7901}ng c\uc0{\u242}n \uc0{\u273}\uc0{\u259}ng k\uc0{\u253} tham gia l\uc0{\u7899}p t\uc0{\u7853}p hu\uc0{\u7845}n PCCC do Ph\uc0{\u242}ng GD\uc0{\u272}T v\uc0{\u224} \uc0{\u273}\uc0{\u7883}a ph\uc0{\u432}\uc0{\u417}ng t\uc0{\u7893} ch\uc0{\u7913}c, sau \uc0{\u273}\uc0{\u243} v\uc0{\u7873} tri\uc0{\u7875}n khai l\uc0{\u7841}i cho CB-GV-CNV t\uc0{\u7841}i \uc0{\u273}\uc0{\u417}n v\uc0{\u7883}. }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1  T\uc0{\u7893} ch\uc0{\u7913}c tuy\uc0{\u234}n truy\uc0{\u7873}n, gi\uc0{\u225}o d\uc0{\u7909}c n\uc0{\u226}ng cao nh\uc0{\u7853}n th\uc0{\u7913}c v\uc0{\u224} th\uc0{\u7921}c h\uc0{\u224}nh v\uc0{\u7873} an to\uc0{\u224}n v\uc0{\u7879} sinh th\uc0{\u7921}c ph\uc0{\u7849}m cho CB-GV-CNV.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1  \uc0{\u272}\uc0{\u259}ng k\uc0{\u253} cho b\uc0{\u7843}o v\uc0{\u7879} \uc0{\u273}\uc0{\u432}\uc0{\u7907}c tham gia c\uc0{\u225}c l\uc0{\u7899}p nghi\uc0{\u7879}p v\uc0{\u7909} b\uc0{\u7843}o v\uc0{\u7879} \uc0{\u273}\uc0{\u7875} ph\uc0{\u7909}c v\uc0{\u7909} t\uc0{\u7889}t cho c\uc0{\u244}ng vi\uc0{\u7879}c b\uc0{\u7843}o v\uc0{\u7879} trong nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.7.5. T\uc0{\u7921} \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} ti\uc0{\u234}u ch\uc0{\u237}: \uc0{\u272}\uc0{\u7841}t}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \b \i 1.8. Ti\uc0{\u234}u ch\uc0{\u237} 8: T\uc0{\u7893} ch\uc0{\u7913}c c\uc0{\u225}c ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng l\uc0{\u7877} h\uc0{\u7897}i, v\uc0{\u259}n ngh\uc0{\u7879}, vui ch\uc0{\u417}i ph\uc0{\u249} h\uc0{\u7907}p v\uc0{\u7899}i \uc0{\u273}i\uc0{\u7873}u ki\uc0{\u7879}n \uc0{\u273}\uc0{\u7883}a ph\uc0{\u432}\uc0{\u417}ng.\i0 \b0 {\rtlch\fcs1 \af31507 \ltrch\fcs0 \insrsid10420566\charrsid10420566 {\*\bkmkstart tieuchuan0tieuchi7}}}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \i a) C\uc0{\u243} n\uc0{\u7897}i dung ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng l\uc0{\u7877} h\uc0{\u7897}i, v\uc0{\u259}n ngh\uc0{\u7879}, vui ch\uc0{\u417}i theo t\uc0{\u7915}ng th\uc0{\u225}ng, t\uc0{\u7915}ng n\uc0{\u259}m h\uc0{\u7885}c v\uc0{\u224} th\uc0{\u7921}c hi\uc0{\u7879}n \uc0{\u273}\uc0{\u250}ng ti\uc0{\u7871}n \uc0{\u273}\uc0{\u7897}, \uc0{\u273}\uc0{\u7841}t hi\uc0{\u7879}u qu\uc0{\u7843}.\i0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \i b) Trong n\uc0{\u259}m h\uc0{\u7885}c t\uc0{\u7893} ch\uc0{\u7913}c \uc0{\u237}t nh\uc0{\u7845}t m\uc0{\u7897}t l\uc0{\u7847}n cho tr\uc0{\u7867} t\uc0{\u7915} 4 tu\uc0{\u7893}i tr\uc0{\u7903} l\uc0{\u234}n tham quan \uc0{\u273}\uc0{\u7883}a danh, di t\uc0{\u237}ch l\uc0{\u7883}ch s\uc0{\u7917}, v\uc0{\u259}n ho\uc0{\u225} \uc0{\u273}\uc0{\u7883}a ph\uc0{\u432}\uc0{\u417}ng ho\uc0{\u7863}c m\uc0{\u7901}i ngh\uc0{\u7879} nh\uc0{\u226}n h\uc0{\u432}\uc0{\u7899}ng d\uc0{\u7851}n tr\uc0{\u7867} l\uc0{\u224}m \uc0{\u273}\uc0{\u7891} ch\uc0{\u417}i d\uc0{\u226}n gian.\i0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \i c) Ph\uc0{\u7893} bi\uc0{\u7871}n, h\uc0{\u432}\uc0{\u7899}ng d\uc0{\u7851}n cho tr\uc0{\u7867} c\uc0{\u225}c tr\uc0{\u242} ch\uc0{\u417}i d\uc0{\u226}n gian, c\uc0{\u225}c b\uc0{\u224}i ca dao, \uc0{\u273}\uc0{\u7891}ng dao, b\uc0{\u224}i h\uc0{\u225}t d\uc0{\u226}n ca ph\uc0{\u249} h\uc0{\u7907}p.\i0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.8.1. M\uc0{\u244} t\uc0{\u7843} hi\uc0{\u7879}n tr\uc0{\u7841}ng}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \uc0{\u160}Nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng th\uc0{\u432}\uc0{\u7901}ng xuy\uc0{\u234}n t\uc0{\u7893} ch\uc0{\u7913}c th\uc0{\u7921}c hi\uc0{\u7879}n c\uc0{\u225}c ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng cho tr\uc0{\u7867} tham gia sinh ho\uc0{\u7841}t, vui ch\uc0{\u417}i, v\uc0{\u259}n ngh\uc0{\u7879} trong c\uc0{\u225}c ng\uc0{\u224}y l\uc0{\u7877} h\uc0{\u7897}i nh\uc0{\u432} \uc0{\u8220}Ng\uc0{\u224}y h\uc0{\u7897}i b\uc0{\u233} \uc0{\u273}\uc0{\u7871}n tr\uc0{\u432}\uc0{\u7901}ng; ng\uc0{\u224}y 20/11; ng\uc0{\u224}y h\uc0{\u7897}i b\uc0{\u233} m\uc0{\u7915}ng xu\uc0{\u226}n, tham gia li\uc0{\u234}n hoan Ti\uc0{\u7871}ng h\uc0{\u225}t h\uc0{\u7885}c sinh do PGD&\uc0{\u272}T t\uc0{\u7893} ch\uc0{\u7913}c\uc0{\u8230}\uc0{\u8221} ngo\uc0{\u224}i ra l\uc0{\u242}ng gh\uc0{\u233}p \uc0{\u226}m nh\uc0{\u7841}c, bi\uc0{\u7875}u di\uc0{\u7877}n v\uc0{\u259}n ngh\uc0{\u7879} qua c\uc0{\u225}c ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng h\uc0{\u7885}c t\uc0{\u7853}p trong c\uc0{\u225}c ch\uc0{\u7911} \uc0{\u273}\uc0{\u7873} trong n\uc0{\u259}m h\uc0{\u7885}c. Qua th\uc0{\u7921}c hi\uc0{\u7879}n Phong tr\uc0{\u224}o thi \uc0{\u273}ua \uc0{\u8220}x\uc0{\u226}y d\uc0{\u7921}ng tr\uc0{\u432}\uc0{\u7901}ng h\uc0{\u7885}c th\uc0{\u226}n thi\uc0{\u7879}n, h\uc0{\u7885}c sinh t\uc0{\u237}ch c\uc0{\u7921}c\uc0{\u8221}\b [H1.1.08.01].\b0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 lam th\uc0{\u7855}ng c\uc0{\u7843}nh c\uc0{\u7911}a \uc0{\u273}\uc0{\u7883}a ph\uc0{\u432}\uc0{\u417}ng v\uc0{\u224} t\uc0{\u7893} ch\uc0{\u7913}c cho tr\uc0{\u7867} l\uc0{\u7899}p l\uc0{\u225} \uc0{\u273}i tham quan \uc0{\u273}\uc0{\u7883}a danh c\uc0{\u7911}a \uc0{\u273}\uc0{\u7883}a ph\uc0{\u432}\uc0{\u417}ng nh\uc0{\u432}: V\uc0{\u432}\uc0{\u7901}n c\uc0{\u226}y cao su c\uc0{\u7911}a n\uc0{\u244}ng tr\uc0{\u432}\uc0{\u7901}ng D\uc0{\u7847}u D\uc0{\u226}y, Tham quan tr\uc0{\u432}\uc0{\u7901}ng ti\uc0{\u7875}u h\uc0{\u7885}c; l\uc0{\u242}ng gh\uc0{\u233}p qua c\uc0{\u225}c b\uc0{\u224}i th\uc0{\u417}, ca dao, nh\uc0{\u7841}c theo t\uc0{\u7915}ng ch\uc0{\u7911} \uc0{\u273}\uc0{\u7873}. Ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng ngo\uc0{\u224}i tr\uc0{\u7901}i c\uc0{\u243} l\uc0{\u242}ng gh\uc0{\u233}p t\uc0{\u7893} ch\uc0{\u7913}c c\uc0{\u225}c tr\uc0{\u242} ch\uc0{\u417}i d\uc0{\u226}n gian cho tr\uc0{\u7867}. H\uc0{\u432}\uc0{\u7899}ng d\uc0{\u7851}n tr\uc0{\u7867} l\uc0{\u224}m m\uc0{\u7897}t s\uc0{\u7889} \uc0{\u272}D\uc0{\u272}C t\uc0{\u7921} t\uc0{\u7841}o qua gi\uc0{\u7901} ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng g\uc0{\u243}c, ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng ngo\uc0{\u224}i tr\uc0{\u7901}i\uc0{\u160} \b [H1.1.08.02]\b0 .}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \uc0{\u160} Ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng ngo\uc0{\u224}i tr\uc0{\u7901}i c\uc0{\u243} l\uc0{\u242}ng gh\uc0{\u233}p t\uc0{\u7893} ch\uc0{\u7913}c c\uc0{\u225}c tr\uc0{\u242} ch\uc0{\u417}i d\uc0{\u226}n gian cho tr\uc0{\u7867}. H\uc0{\u432}\uc0{\u7899}ng d\uc0{\u7851}n tr\uc0{\u7867} l\uc0{\u224}m m\uc0{\u7897}t s\uc0{\u7889} \uc0{\u272}D\uc0{\u272}C t\uc0{\u7921} t\uc0{\u7841}o qua gi\uc0{\u7901} ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng g\uc0{\u243}c, ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng ngo\uc0{\u224}i tr\uc0{\u7901}i \b [H1.1.08.03].\b0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.8.2. \uc0{\u272}i\uc0{\u7875}m m\uc0{\u7841}nh}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Tr\uc0{\u432}\uc0{\u7901}ng c\uc0{\u243} s\uc0{\u226}n ch\uc0{\u417}i r\uc0{\u7897}ng r\uc0{\u227}i, tho\uc0{\u225}ng m\uc0{\u225}t thu\uc0{\u7853}n l\uc0{\u7907}i cho vi\uc0{\u7879}c t\uc0{\u7893} ch\uc0{\u7913}c c\uc0{\u225}c tr\uc0{\u242} ch\uc0{\u417}i d\uc0{\u226}n gian cho tr\uc0{\u7867}, l\uc0{\u7899}p h\uc0{\u7885}c lu\uc0{\u244}n xanh, s\uc0{\u7841}ch, \uc0{\u273}\uc0{\u7865}p, an to\uc0{\u224}n ph\uc0{\u249} h\uc0{\u7907}p v\uc0{\u7899}i l\uc0{\u7913}a tu\uc0{\u7893}i m\uc0{\u7847}m non, ph\uc0{\u249} h\uc0{\u7907}p v\uc0{\u7899}i phong tr\uc0{\u224}o thi \uc0{\u273}ua x\uc0{\u226}y d\uc0{\u7921}ng \uc0{\u8220}Tr\uc0{\u432}\uc0{\u7901}ng h\uc0{\u7885}c th\uc0{\u226}n thi\uc0{\u7879}n, h\uc0{\u7885}c sinh t\uc0{\u237}ch c\uc0{\u7921}c\uc0{\u8221} \uc0{\u272}\uc0{\u7897}i ng\uc0{\u361} Gi\uc0{\u225}o vi\uc0{\u234}n tr\uc0{\u7867}, n\uc0{\u259}ng \uc0{\u273}\uc0{\u7897}ng, t\uc0{\u237}ch c\uc0{\u7921}c s\uc0{\u432}u t\uc0{\u7847}m nhi\uc0{\u7873}u tr\uc0{\u242} ch\uc0{\u417}i m\uc0{\u7899}i, tr\uc0{\u7867} \uc0{\u273}\uc0{\u432}\uc0{\u7907}c sinh ho\uc0{\u7841}t v\uc0{\u7899}i nhi\uc0{\u7873}u n\uc0{\u7897}i dung phong ph\uc0{\u250} trong c\uc0{\u225}c ng\uc0{\u224}y l\uc0{\u7877} h\uc0{\u7897}i, CMHS ph\uc0{\u7889}i h\uc0{\u7907}p tham gia trong c\uc0{\u225}c phong tr\uc0{\u224}o h\uc0{\u7897}i thao, v\uc0{\u259}n ngh\uc0{\u7879}. }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1  Gi\uc0{\u225}o vi\uc0{\u234}n t\uc0{\u237}ch c\uc0{\u7921}c s\uc0{\u432}u t\uc0{\u7847}m nhi\uc0{\u7873}u tr\uc0{\u242} ch\uc0{\u417}i d\uc0{\u226}n gian, c\uc0{\u225}c b\uc0{\u224}i \uc0{\u273}\uc0{\u7891}ng dao, b\uc0{\u224}i h\uc0{\u225}t m\uc0{\u7899}i  ***g gh\uc0{\u233}p v\uc0{\u224}o ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng gi\uc0{\u7843}ng d\uc0{\u7841}y. T\uc0{\u7841}o th\uc0{\u234}m g\uc0{\u243}c ch\uc0{\u417}i ngo\uc0{\u224}i s\uc0{\u226}n, b\uc0{\u7893} sung m\uc0{\u7897}t s\uc0{\u7889} \uc0{\u273}\uc0{\u7891} ch\uc0{\u417}i d\uc0{\u226}n gian m\uc0{\u7899}i, nh\uc0{\u7857}m gi\uc0{\u250}p tr\uc0{\u7867} r\uc0{\u232}n luy\uc0{\u7879}n th\uc0{\u234}m k\uc0{\u7929} n\uc0{\u259}ng, t\uc0{\u432} duy, th\uc0{\u7875} l\uc0{\u7921}c. Th\uc0{\u432}\uc0{\u7901}ng xuy\uc0{\u234}n h\uc0{\u432}\uc0{\u7899}ng d\uc0{\u7851}n v\uc0{\u224} t\uc0{\u7893} ch\uc0{\u7913}c cho tr\uc0{\u7867} ch\uc0{\u417}i v\uc0{\u224}o c\uc0{\u225}c ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng trong ng\uc0{\u224}y, t\uc0{\u7841}o s\uc0{\u7921} th\uc0{\u226}n thi\uc0{\u7871}t gi\uc0{\u7919}a tr\uc0{\u7867} v\uc0{\u7899}i tr\uc0{\u7867}, gi\uc0{\u7919}a c\uc0{\u244} gi\uc0{\u225}o v\uc0{\u7899}i tr\uc0{\u7867} \uc0{\u273}\uc0{\u7875} gi\uc0{\u250}p tr\uc0{\u7867} y\uc0{\u234}u tr\uc0{\u432}\uc0{\u7901}ng, m\uc0{\u7871}n l\uc0{\u7899}p v\uc0{\u224} ham th\uc0{\u237}ch \uc0{\u273}\uc0{\u7871}n tr\uc0{\u432}\uc0{\u7901}ng.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.8.3. \uc0{\u272}i\uc0{\u7875}m y\uc0{\u7871}u}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Tr\uc0{\u432}\uc0{\u7901}ng ch\uc0{\u432}a c\uc0{\u243} \uc0{\u273}i\uc0{\u7873}u ki\uc0{\u7879}n cho tr\uc0{\u7867} \uc0{\u273}i tham quan \uc0{\u273}\uc0{\u7883}a danh, di t\uc0{\u237}ch l\uc0{\u7883}ch s\uc0{\u7917} c\uc0{\u7911}a \uc0{\u272}\uc0{\u7891}ng nai. Ch\uc0{\u7881} ***g gh\uc0{\u233}p tr\uc0{\u242} chuy\uc0{\u7879}n qua ti\uc0{\u7871}t h\uc0{\u7885}c v\uc0{\u224} xem tranh \uc0{\u7843}nh qua tivi, b\uc0{\u259}ng \uc0{\u273}\uc0{\u297}a.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.8.4. K\uc0{\u7871} ho\uc0{\u7841}ch c\uc0{\u7843}i ti\uc0{\u7871}n ch\uc0{\u7845}t l\uc0{\u432}\uc0{\u7907}ng}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Ti\uc0{\u7871}p t\uc0{\u7909}c \uc0{\u273}\uc0{\u7847}u t\uc0{\u432} m\uc0{\u7897}t s\uc0{\u7889} m\uc0{\u244}i tr\uc0{\u432}\uc0{\u7901}ng ch\uc0{\u417}i d\uc0{\u226}n gian trong s\uc0{\u226}n tr\uc0{\u432}\uc0{\u7901}ng cho tr\uc0{\u7867} ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng. Ti\uc0{\u7871}p t\uc0{\u7909}c th\uc0{\u7921}c hi\uc0{\u7879}n hi\uc0{\u7879}u qu\uc0{\u7843} phong tr\uc0{\u224}o thi \uc0{\u273}ua x\uc0{\u226}y d\uc0{\u7921}ng \uc0{\u8220}Tr\uc0{\u432}\uc0{\u7901}ng h\uc0{\u7885}c th\uc0{\u226}n thi\uc0{\u7879}n, h\uc0{\u7885}c sinh t\uc0{\u237}ch c\uc0{\u7921}c\uc0{\u8221} x\uc0{\u226}y d\uc0{\u7921}ng m\uc0{\u244}i tr\uc0{\u432}\uc0{\u7901}ng lu\uc0{\u244}n s\uc0{\u7841}ch, \uc0{\u273}\uc0{\u7865}p, an to\uc0{\u224}n ph\uc0{\u249} h\uc0{\u7907}p v\uc0{\u7899}i l\uc0{\u7913}a tu\uc0{\u7893}i m\uc0{\u7847}m non. }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 Gi\uc0{\u225}o vi\uc0{\u234}n t\uc0{\u237}ch c\uc0{\u7921}c \uc0{\u273}\uc0{\u7893}i m\uc0{\u7899}i ph\uc0{\u432}\uc0{\u417}ng ph\uc0{\u225}p t\uc0{\u7893} ch\uc0{\u7913}c ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng, t\uc0{\u237}ch c\uc0{\u7921}c s\uc0{\u432}u t\uc0{\u7847}m nhi\uc0{\u7873}u tr\uc0{\u242} ch\uc0{\u417}i m\uc0{\u7899}i, t\uc0{\u7841}o \uc0{\u273}i\uc0{\u7873}u ki\uc0{\u7879}n cho tr\uc0{\u7867} \uc0{\u273}\uc0{\u432}\uc0{\u7907}c sinh ho\uc0{\u7841}t vui ch\uc0{\u417}i v\uc0{\u7899}i nhi\uc0{\u7873}u n\uc0{\u7897}i dung phong ph\uc0{\u250} trong c\uc0{\u225}c ng\uc0{\u224}y l\uc0{\u7877} h\uc0{\u7897}i v\uc0{\u224} trong c\uc0{\u225}c ch\uc0{\u7911} \uc0{\u273}\uc0{\u7873}.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1  \uc0{\u272}\uc0{\u7897}ng vi\uc0{\u234}n PHHS \uc0{\u273}\uc0{\u243}ng g\uc0{\u243}p kinh ph\uc0{\u237} cho tr\uc0{\u7867} tham quan m\uc0{\u7897}t s\uc0{\u7889} danh lam th\uc0{\u7855}ng c\uc0{\u7843}nh c\uc0{\u7911}a \uc0{\u272}\uc0{\u7891}ng Nai v\uc0{\u224}o cu\uc0{\u7889}i th\uc0{\u225}ng 5 h\uc0{\u224}ng n\uc0{\u259}m \uc0{\u273}\uc0{\u7875} cung c\uc0{\u7845}p cho tr\uc0{\u7867} m\uc0{\u7897}t s\uc0{\u7889} di t\uc0{\u237}ch l\uc0{\u7883}ch s\uc0{\u7917} c\uc0{\u7911}a \uc0{\u273}\uc0{\u7891}ng nai Qua \uc0{\u273}\uc0{\u243} r\uc0{\u232}n k\uc0{\u7929} n\uc0{\u259}ng s\uc0{\u7889}ng cho tr\uc0{\u7867}.}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 1.8.5. T\uc0{\u7921} \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} ti\uc0{\u234}u ch\uc0{\u237}: \uc0{\u272}\uc0{\u7841}t}\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \b K\uc0{\u7871}t lu\uc0{\u7853}n v\uc0{\u7873} Ti\uc0{\u234}u chu\uc0{\u7849}n 1: \b0 }\par \pard \qj \fi737 \sa120 \sl408 {\fs28 \f1 \uc0{\u272}\uc0{\u7875} \uc0{\u273}\uc0{\u7843}m b\uc0{\u7843}o \uc0{\u273}\uc0{\u7911} v\uc0{\u7873} s\uc0{\u7889} l\uc0{\u432}\uc0{\u7907}ng, ch\uc0{\u7845}t l\uc0{\u432}\uc0{\u7907}ng. Nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng lu\uc0{\u244}n t\uc0{\u7841}o \uc0{\u273}i\uc0{\u7873}u ki\uc0{\u7879}n cho CB \uc0{\u8211} GV \uc0{\u8211} CNV \uc0{\u273}\uc0{\u432}\uc0{\u7907}c h\uc0{\u7885}c t\uc0{\u7853}p v\uc0{\u224} b\uc0{\u7891}i d\uc0{\u432}\uc0{\u7905}ng n\uc0{\u226}ng cao tr\uc0{\u236}nh \uc0{\u273}\uc0{\u7897} chuy\uc0{\u234}n m\uc0{\u244}n \uc0{\u273}\uc0{\u224}o t\uc0{\u7841}o, tr\uc0{\u234}n 85% \uc0{\u273}\uc0{\u7897}i ng\uc0{\u361} gi\uc0{\u225}o vi\uc0{\u234}n c\uc0{\u243} tr\uc0{\u236}nh \uc0{\u273}\uc0{\u7897} tr\uc0{\u234}n chu\uc0{\u7849}n v\uc0{\u7899}i ***g y\uc0{\u234}u ngh\uc0{\u7873}, m\uc0{\u7871}n tr\uc0{\u7867} gi\uc0{\u225}o vi\uc0{\u234}n \uc0{\u273}\uc0{\u227} v\uc0{\u7853}n d\uc0{\u7909}ng s\uc0{\u225}ng t\uc0{\u7841}o trong gi\uc0{\u7843}ng d\uc0{\u7841}y \uc0{\u273}\uc0{\u7875} ph\uc0{\u225}t huy hi\uc0{\u7879}u qu\uc0{\u7843} c\uc0{\u225}c phong tr\uc0{\u224}o thi \uc0{\u273}ua c\uc0{\u7911}a ng\uc0{\u224}nh. T\uc0{\u7853}p th\uc0{\u7875} CB-GV-CNV lu\uc0{\u244}n ph\uc0{\u7845}n \uc0{\u273}\uc0{\u7845}u \uc0{\u273}\uc0{\u7875} ch\uc0{\u7845}t l\uc0{\u432}\uc0{\u7907}ng gi\uc0{\u225}o d\uc0{\u7909}c c\uc0{\u7911}a nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng ng\uc0{\u224}y c\uc0{\u224}ng n\uc0{\u226}ng cao, t\uc0{\u7841}o \uc0{\u273}\uc0{\u432}\uc0{\u7907}c s\uc0{\u7921} t\uc0{\u237}n nhi\uc0{\u7879}m c\uc0{\u7911}a Ng\uc0{\u224}nh, ch\uc0{\u237}nh quy\uc0{\u7873}n \uc0{\u273}\uc0{\u7883}a ph\uc0{\u432}\uc0{\u417}ng, CMHS t\uc0{\u7841}o \uc0{\u273}i\uc0{\u7873}u ki\uc0{\u7879}n nh\uc0{\u7857}m th\uc0{\u250}c \uc0{\u273}\uc0{\u7849}y c\uc0{\u225}c ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng ch\uc0{\u432}\uc0{\u417}ng tr\uc0{\u236}nh gi\uc0{\u225}o d\uc0{\u7909}c c\uc0{\u7911}a nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng, \uc0{\u273}\uc0{\u225}p \uc0{\u7913}ng v\uc0{\u7899}i nhu c\uc0{\u7847}u d\uc0{\u7841}y v\uc0{\u224} h\uc0{\u7885}c.Th\uc0{\u7921}c hi\uc0{\u7879}n t\uc0{\u7889}t theo ch\uc0{\u432}\uc0{\u417}ng tr\uc0{\u236}nh CSGD M\uc0{\u7847}m Non M\uc0{\u7899}i. B\uc0{\u7889} tr\uc0{\u237} ph\uc0{\u249} h\uc0{\u7907}p theo nhu c\uc0{\u7847}u tr\uc0{\u23