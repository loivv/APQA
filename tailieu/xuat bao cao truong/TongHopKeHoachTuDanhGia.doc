\par \pard \ql \fi737 \sl408 {\fs28 \f0 \b 2. T\uc0{\u7853}p hu\uc0{\u7845}n nghi\uc0{\u7879}p v\uc0{\u7909} t\uc0{\u7921} \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225}}\par \pard \ql \fi737 \sl408 {\fs28 \f1 a) Th\uc0{\u7901}i gian}\par \pard \ql \fi737 \sl408 {\fs28 \f1 V\uc0{\u224}o l\uc0{\u250}c 8h00 ng\uc0{\u224}y 09 th\uc0{\u225}ng 10 n\uc0{\u259}m 2015}\par \pard \ql \fi737 \sl408 {\fs28 \f1 b) Th\uc0{\u224}nh ph\uc0{\u7847}n}\par \pard \ql \fi737 \sl408 {\fs28 \f1 C\uc0{\u225}c th\uc0{\u224}nh vi\uc0{\u234}n trong H\uc0{\u7897}i \uc0{\u273}\uc0{\u7891}ng t\uc0{\u7921} \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225}, nh\uc0{\u243}m th\uc0{\u432} k\uc0{\u253} v\uc0{\u224} c\uc0{\u225}c nh\uc0{\u243}m c\uc0{\u244}ng t\uc0{\u225}c.}\par \pard \ql \fi737 \sl408 {\fs28 \f1 c) N\uc0{\u7897}i dung}\par \pard \ql \fi737 \sl408 {\fs28 \f1 1. T\uc0{\u7893} ch\uc0{\u7913}c t\uc0{\u7853}p hu\uc0{\u7845}n v\uc0{\u7873} nghi\uc0{\u7879}p v\uc0{\u7909} t\uc0{\u7921} \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} cho CB-GV-NV t\uc0{\u7841}i tr\uc0{\u432}\uc0{\u7901}ng.}\par \pard \ql \fi737 \sl408 {\fs28 \f1 B\uc0{\u224}: Nguy\uc0{\u7877}n th\uc0{\u7883} B\uc0{\u237}ch Nga th\uc0{\u244}ng qua tr\uc0{\u432}\uc0{\u7899}c h\uc0{\u7897}i \uc0{\u273}\uc0{\u7891}ng m\uc0{\u7909}c \uc0{\u273}\uc0{\u237}ch v\uc0{\u224} nguy\uc0{\u234}n t\uc0{\u7855}c t\uc0{\u7921} \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} ch\uc0{\u7845}t l\uc0{\u432}\uc0{\u7907}ng gi\uc0{\u225}o d\uc0{\u7909}c tr\uc0{\u432}\uc0{\u7901}ng m\uc0{\u7847}m non.}\par \pard \ql \fi737 \sl408 {\fs28 \f1 * M\uc0{\u7909}c \uc0{\u273}\uc0{\u237}ch: Ki\uc0{\u7875}m \uc0{\u273}\uc0{\u7883}nh ch\uc0{\u7845}t l\uc0{\u432}\uc0{\u7907}ng gi\uc0{\u225}o d\uc0{\u7909}c nh\uc0{\u7857}m gi\uc0{\u250}p tr\uc0{\u432}\uc0{\u7901}ng m\uc0{\u7847}m non x\uc0{\u225}c \uc0{\u273}\uc0{\u7883}nh m\uc0{\u7913}c \uc0{\u273}\uc0{\u7897} \uc0{\u273}\uc0{\u225}p \uc0{\u7913}ng m\uc0{\u7909}c ti\uc0{\u234}u gi\uc0{\u225}o d\uc0{\u7909}c trong t\uc0{\u7915}ng giai \uc0{\u273}o\uc0{\u7841}n, \uc0{\u273}\uc0{\u7875} x\uc0{\u226}y d\uc0{\u7921}ng k\uc0{\u7871} ho\uc0{\u7841}ch c\uc0{\u7843}i ti\uc0{\u7871}n, n\uc0{\u226}ng cao ch\uc0{\u7845}t l\uc0{\u432}\uc0{\u7907}ng c\uc0{\u225}c ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng gi\uc0{\u225}o d\uc0{\u7909}c th\uc0{\u244}ng b\uc0{\u225}o c\uc0{\u244}ng khai v\uc0{\u7899}i c\uc0{\u225}c c\uc0{\u417} quan qu\uc0{\u7843}n l\uc0{\u253} nh\uc0{\u224} n\uc0{\u432}\uc0{\u7899}c v\uc0{\u224} x\uc0{\u227} h\uc0{\u7897}i v\uc0{\u7873} th\uc0{\u7921}c tr\uc0{\u7841}ng ch\uc0{\u7845}t l\uc0{\u432}\uc0{\u7907}ng c\uc0{\u7911}a tr\uc0{\u432}\uc0{\u7901}ng m\uc0{\u7847}m non \uc0{\u273}\uc0{\u7875} c\uc0{\u417} quan qu\uc0{\u7843}n l\uc0{\u253} nh\uc0{\u224} n\uc0{\u432}\uc0{\u7899}c \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} v\uc0{\u224} c\uc0{\u244}ng nh\uc0{\u7853}n tr\uc0{\u432}\uc0{\u7901}ng m\uc0{\u7847}m non \uc0{\u273}\uc0{\u7841}t ti\uc0{\u234}u chu\uc0{\u7849}n ch\uc0{\u7845}t l\uc0{\u432}\uc0{\u7907}ng gi\uc0{\u225}o d\uc0{\u7909}c.}\par \pard \ql \fi737 \sl408 {\fs28 \f1 * Nguy\uc0{\u234}n t\uc0{\u7855}c}\par \pard \ql \fi737 \sl408 {\fs28 \f1 + \uc0{\u272}\uc0{\u7897}c l\uc0{\u7853}p, kh\uc0{\u225}ch quan, \uc0{\u273}\uc0{\u250}ng ph\uc0{\u225}p lu\uc0{\u7853}t}\par \pard \ql \fi737 \sl408 {\fs28 \f1 + Trung th\uc0{\u7921}c, c\uc0{\u244}ng khai, minh b\uc0{\u7841}ch}\par \pard \ql \fi737 \sl408 {\fs28 \f1 2. H\uc0{\u7897}i \uc0{\u273}\uc0{\u7891}ng t\uc0{\u7921} \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225}}\par \pard \ql \fi737 \sl408 {\fs28 \f1 H\uc0{\u7897}i \uc0{\u273}\uc0{\u7891}ng \uc0{\u273}\uc0{\u432}\uc0{\u7907}c \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} theo quy\uc0{\u7871}t \uc0{\u273}\uc0{\u7883}nh s\uc0{\u7889} 76.a ng\uc0{\u224}y 9 th\uc0{\u225}ng 10 n\uc0{\u259}m 2015 c\uc0{\u7911}a hi\uc0{\u7879}u tr\uc0{\u432}\uc0{\u7903}ng tr\uc0{\u432}\uc0{\u7901}ng Mn H\uc0{\u432}\uc0{\u7899}ng D\uc0{\u432}\uc0{\u417}ng  g\uc0{\u7891}m 10 th\uc0{\u224}nh vi\uc0{\u234}n (c\uc0{\u243} quy\uc0{\u7871}t \uc0{\u27