\pard \ql \fi737 \sl408 {\fs28 \f0 \b I. M\uc0{\u7908}C \uc0{\u272}\uc0{\u205}CH V\uc0{\u192} PH\uc0{\u7840}M VI}\par \pard \ql \fi737 \sl408 {\fs28 \f1 1. M\uc0{\u7909}c \uc0{\u273}\uc0{\u237}ch t\uc0{\u7921} \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225}}\par \pard \ql \fi737 \sl408 {\fs28 \f1 1. M\uc0{\u7909}c \uc0{\u273}\uc0{\u237}ch c\uc0{\u7911}a t\uc0{\u7921} \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} l\uc0{\u224} xem x\uc0{\u233}t, ki\uc0{\u7875}m tra, \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} theo Th\uc0{\u244}ng t\uc0{\u432} s\uc0{\u7889} 25/2014/TT-BGD\uc0{\u272}T \uc0{\u273}\uc0{\u7875} x\uc0{\u225}c \uc0{\u273}\uc0{\u7883}nh th\uc0{\u7921}c tr\uc0{\u7841}ng ch\uc0{\u7845}t l\uc0{\u432}\uc0{\u7907}ng, hi\uc0{\u7879}u qu\uc0{\u7843} gi\uc0{\u225}o d\uc0{\u7909}c, nh\uc0{\u226}n l\uc0{\u7921}c, c\uc0{\u417} s\uc0{\u7903} v\uc0{\u7853}t ch\uc0{\u7845}t, t\uc0{\u7915} \uc0{\u273}\uc0{\u243} th\uc0{\u7921}c hi\uc0{\u7879}n c\uc0{\u225}c bi\uc0{\u7879}n ph\uc0{\u225}p c\uc0{\u7843}i ti\uc0{\u7871}n, n\uc0{\u226}ng cao ch\uc0{\u7845}t l\uc0{\u432}\uc0{\u7907}ng gi\uc0{\u225}o d\uc0{\u7909}c c\uc0{\u7911}a nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng; \uc0{\u273}\uc0{\u7875} gi\uc0{\u7843}i tr\uc0{\u236}nh v\uc0{\u7899}i c\uc0{\u225}c c\uc0{\u417} quan ch\uc0{\u7913}c n\uc0{\u259}ng, v\uc0{\u7899}i x\uc0{\u227} h\uc0{\u7897}i v\uc0{\u7873} th\uc0{\u7921}c tr\uc0{\u7841}ng ch\uc0{\u7845}t l\uc0{\u432}\uc0{\u7907}ng gi\uc0{\u225}o d\uc0{\u7909}c v\uc0{\u224}  \uc0{\u273}\uc0{\u7875} c\uc0{\u417} quan ch\uc0{\u7913}c n\uc0{\u259}ng \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} v\uc0{\u224} c\uc0{\u244}ng nh\uc0{\u7853}n nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng \uc0{\u273}\uc0{\u7841}t ti\uc0{\u234}u chu\uc0{\u7849}n ch\uc0{\u7845}t l\uc0{\u432}\uc0{\u7907}ng gi\uc0{\u225}o d\uc0{\u7909}c.}\par \pard \ql \fi737 \sl408 {\fs28 \f1 2. Ph\uc0{\u7841}m vi t\uc0{\u7921} \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225}}\par \pard \ql \fi737 \sl408 {\fs28 \f1 2. Ph\uc0{\u7841}m vi t\uc0{\u7921} \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} l\uc0{\u224} to\uc0{\u224}n b\uc0{\u7897} c\uc0{\u225}c ho\uc0{\u7841}t \uc0{\u273}\uc0{\u7897}ng c\uc0{\u7911}a nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng theo c\uc0{\u225}c ti\uc0{\u234}u chu\uc0{\u7849}n \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} ch\uc0{\u7845}t l\uc0{\u432}\uc0{\u7907}ng gi\uc0{\u225}o d\uc0{\u7909}c do B\uc0{\u7897} Gi\uc0{\u225}o d\uc0{\u7909}c v\uc0{\u224} \uc0{\u272}\uc0{\u224}o t\uc0{\u7841}o ban h\uc0{\u224}nh.sdfsdf}\par \pard \ql \fi737 \sl408 {\fs28 \f1 3. Y\uc0{\u234}u c\uc0{\u7847}u}\par \pard \ql \fi737 \sl408 {\fs28 \f1 a) Kh\uc0{\u225}ch quan v\uc0{\u224} trung th\uc0{\u7921}c;}\par \pard \ql \fi737 \sl408 {\fs28 \f1 Th\uc0{\u224}nh l\uc0{\u7853}p H\uc0{\u7897}i \uc0{\u273}\uc0{\u7891}ng t\uc0{\u7921} \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} ch\uc0{\u7845}t l\uc0{\u432}\uc0{\u7907}ng g\uc0{\u7891}m c\uc0{\u225}n b\uc0{\u7897} ch\uc0{\u7911} ch\uc0{\u7889}t c\uc0{\u7911}a tr\uc0{\u432}\uc0{\u7901}ng, tr\uc0{\u432}\uc0{\u7903}ng c\uc0{\u225}c \uc0{\u273}o\uc0{\u224}n th\uc0{\u7875}, c\uc0{\u225}c b\uc0{\u7897} ph\uc0{\u7853}n ch\uc0{\u7913}c n\uc0{\u259}ng, C\uc0{\u244}ng \uc0{\u273}o\uc0{\u224}n, thanh tra nh\uc0{\u226}n d\uc0{\u226}n. Ban th\uc0{\u432} k\uc0{\u253} l\uc0{\u224} c\uc0{\u225}c c\uc0{\u225}n b\uc0{\u7897}, gi\uc0{\u225}o vi\uc0{\u234}n c\uc0{\u243} kinh nghi\uc0{\u7879}m. H\uc0{\u7897}i \uc0{\u273}\uc0{\u7891}ng t\uc0{\u7921} \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} c\uc0{\u7911}a nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng c\uc0{\u243} tr\uc0{\u225}ch nhi\uc0{\u7879}m x\uc0{\u226}y d\uc0{\u7921}ng k\uc0{\u7871} ho\uc0{\u7841}ch t\uc0{\u7893}ng th\uc0{\u7875} v\uc0{\u224} k\uc0{\u7871} ho\uc0{\u7841}ch chi ti\uc0{\u7871}t, ph\uc0{\u226}n c\uc0{\u244}ng c\uc0{\u244}ng vi\uc0{\u7879}c c\uc0{\u7909} th\uc0{\u7875} cho t\uc0{\u7915}ng th\uc0{\u224}nh vi\uc0{\u234}n. }\par \pard \ql \fi737 \sl408 {\fs28 \f1 b) C\uc0{\u244}ng khai v\uc0{\u224} minh b\uc0{\u7841}ch;}\par \pard \ql \fi737 \sl408 {\fs28 \f1 Trong qu\uc0{\u225} tr\uc0{\u236}nh t\uc0{\u7921} \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225}, c\uc0{\u225}c nh\uc0{\u243}m \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} v\uc0{\u224} ban th\uc0{\u432} k\uc0{\u253} \uc0{\u273}\uc0{\u227} ti\uc0{\u7871}n h\uc0{\u224}nh thu th\uc0{\u7853}p, x\uc0{\u7917} l\uc0{\u253}, ph\uc0{\u226}n t\uc0{\u237}ch c\uc0{\u225}c th\uc0{\u244}ng tin, minh ch\uc0{\u7913}ng, \uc0{\u273}\uc0{\u7889}i chi\uc0{\u7871}u v\uc0{\u7899}i c\uc0{\u225}c ti\uc0{\u234}u chu\uc0{\u7849}n, ti\uc0{\u234}u ch\uc0{\u237} c\uc0{\u7911}a B\uc0{\u7897} ti\uc0{\u234}u chu\uc0{\u7849}n ki\uc0{\u7875}m \uc0{\u273}\uc0{\u7883}nh ch\uc0{\u7845}t l\uc0{\u432}\uc0{\u7907}ng gi\uc0{\u225}o d\uc0{\u7909}c tr\uc0{\u432}\uc0{\u7901}ng m\uc0{\u7847}m non. Tr\uc0{\u234}n c\uc0{\u417} s\uc0{\u7903} \uc0{\u273}\uc0{\u243} ph\uc0{\u226}n t\uc0{\u237}ch \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} c\uc0{\u225}c \uc0{\u273}i\uc0{\u7875}m m\uc0{\u7841}nh v\uc0{\u224} nh\uc0{\u7919}ng t\uc0{\u7891} t\uc0{\u7841}i \uc0{\u273}\uc0{\u7875} l\uc0{\u224}m c\uc0{\u259}n c\uc0{\u7913} x\uc0{\u226}y d\uc0{\u7921}ng k\uc0{\u7871} ho\uc0{\u7841}ch t\uc0{\u236}m bi\uc0{\u7879}n ph\uc0{\u225}p \uc0{\u273}\uc0{\u7875} kh\uc0{\u7855}c ph\uc0{\u7909}c nh\uc0{\u7919}ng t\uc0{\u7891}n t\uc0{\u7841}i nh\uc0{\u7857}m c\uc0{\u7843}i ti\uc0{\u7871}n, n\uc0{\u226}ng cao ch\uc0{\u7845}t l\uc0{\u432}\uc0{\u7907}ng gi\uc0{\u225}o d\uc0{\u7909}c c\uc0{\u7911}a nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng. Ban th\uc0{\u432} k\uc0{\u253} c\uc0{\u243} tr\uc0{\u225}ch nhi\uc0{\u7879}m t\uc0{\u7853}p h\uc0{\u7907}p c\uc0{\u225}c phi\uc0{\u7871}u t\uc0{\u7921} \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} theo t\uc0{\u7915}ng ti\uc0{\u234}u chu\uc0{\u7849}n v\uc0{\u224} vi\uc0{\u7871}t b\uc0{\u225}o c\uc0{\u225}o t\uc0{\u7921} \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225}. Sau khi vi\uc0{\u7871}t b\uc0{\u225}o c\uc0{\u225}o, nh\uc0{\u224} tr\uc0{\u432}\uc0{\u7901}ng s\uc0{\u7869} c\uc0{\u244}ng khai b\uc0{\u225}o c\uc0{\u225}o t\uc0{\u7921} \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} kh\uc0{\u225}ch quan v\uc0{\u224} trung th\uc0{\u7921}c.}\par \pard \ql \fi737 \sl408 {\fs28 \f0 \b II. N\uc0{\u7896}I DUNG}\par \pard \ql \fi737 \sl408 {\fs28 \f0 \b 1. Ph\uc0{\u226}n c\uc0{\u244}ng nhi\uc0{\u7879}m v\uc0{\u7909} cho th\uc0{\u224}nh vi\uc0{\u234}n h\uc0{\u7897}i \uc0{\u273}\uc0{\u7891}ng t\uc0{\u7921} \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225}}\par \pard \ql \fi737 \sl408 {\fs28 \f1 a) Th\uc0{\u224}nh vi\uc0{\u234}n h\uc0{\u7897}i \uc0{\u273}\uc0{\u7891}ng t\uc0{\u7921} \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225}}\par \pard \ql \fi737 \sl408 {\fs28 \f1 1.	H\uc0{\u7897}i \uc0{\u273}\uc0{\u7891}ng t\uc0{\u7921} \uc0{\u273}\uc0{\u225}nh gi\uc0{\u225} Tr\uc0{\u432}\uc0{\u7901}ng MN H\uc0{\u432}\uc0{\u7899}ng D\uc0{\u432}\uc0{\u417}ng \uc0{\u273}\uc0{\u432}\uc0{\u7907}c th\uc0{\u224}nh l\uc0{\u7853}p theo Q\uc0{\u272} s\uc0{\u7889} 76.a/Q\uc0{\u272}- MNHD ng\uc0{\u224}y 9/10/2015 c\uc0{\u7911}a hi\uc0{\u7879}u tr\uc0{\u432}\uc0{\u7903}ng Tr\uc0{\u432}\uc0{\u7901}ng MN H\uc0{\u432}\uc0{\u7899}ng D\uc0{\u432}\uc0{\u417}ng H\uc0{\u7897}i \uc0{\u273}\uc0{\u7891}ng g\uc0{\u7891}m 10 th\uc0{\u224}nh vi\uc0{\u234}n}\par \pard \ql {\par}\trowd\trowd\trql\trrh397\clvertalc\clbrdrl\brdrs\brdrw20\brsp0\brdrcf1 \clbrdrr\brdrs\brdrw20\brsp0\brdrcf1 \clbrdrt\brdrs\brdrw20\brsp0\brdrcf1 \clbrdrb\brdrs\brdrw20\brsp0\brdrcf1 \cellx851\clvertalc\clbrdrl\brdrs\brdrw20\brsp0\brdrcf1 \clbrdrr\brdrs\brdrw20\brsp0\brdrcf1 \clbrdrt\brdrs\brdrw20\brsp0\brdrcf1 \clbrdrb\brdrs\brdrw20\brsp0\brdrcf1 \cellx3403\clvertalc\clbrdrl\brdrs\brdrw20\brsp0\brdrcf1 \clbrdrr\brdrs\brdrw20\brsp0\brdrcf1 \clbrdrt\brdrs\brdrw20\brsp0\brdrcf1 \clbrdrb\brdrs\brdrw20\brsp0\brdrcf1 \cellx6238\clvertalc\clbrdrl\brdrs\brdrw20\brsp0\brdrcf1 \clbrdrr\brdrs\brdrw20\brsp0\brdrcf1 \clbrdrt\brdrs\brdrw20\brsp0\brdrcf1 \clbrdrb\brdrs\brdrw20\brsp0\brdrcf1 \cellx9073

